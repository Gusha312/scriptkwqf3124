-- Абсолютный минимум
local r,t,p=game:GetService("RunService"),0,{} for i=1,10 do p[i]=Instance.new("Part") p[i].Size,p[i].Anchored,p[i].CanCollide,p[i].Material,p[i].BrickColor,p[i].Parent=Vector3.new(1,1,1),true,false,Enum.Material.Neon,BrickColor.random(),workspace end r.Heartbeat:Connect(function(d) t=t+d for i=1,10 do local a=c.CFrame.Position p[i].Position=a+Vector3.new(math.cos(t+i)*5,math.sin(t*2)*3,math.sin(t+i)*5) end end)
