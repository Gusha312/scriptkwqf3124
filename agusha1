-- –°–∫—Ä–∏–ø—Ç –¥–ª—è agusha1 - –ö–ò–î–ê–ï–¢ —Ç—Ä–µ–π–¥ kaique_best
-- –≠—Ç–æ—Ç –∏–≥—Ä–æ–∫ –±—É–¥–µ—Ç –æ—Ç–ø—Ä–∞–≤–ª—è—Ç—å —Ç–æ–∫–µ–Ω—ã –ø–æ–ª—É—á–∞—Ç–µ–ª—é

local placeId = 14119723130

if game.PlaceId ~= placeId then
    game:GetService("TeleportService"):Teleport(placeId)
    return
end

print("üîç –ò—â—É –ø–æ–ª—É—á–∞—Ç–µ–ª—è: kaique_best")

-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —Ç—Ä–µ–π–¥–∞
local function sendTrade()
    while true do
        wait(1)
        
        -- –ò—â–µ–º –ø–æ–ª—É—á–∞—Ç–µ–ª—è
        local target = game.Players:FindFirstChild("kaique_best")
        if target then
            print("‚úÖ –ù–∞–π–¥–µ–Ω kaique_best, –æ—Ç–ø—Ä–∞–≤–ª—è—é –∑–∞–ø—Ä–æ—Å...")
            
            -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –∑–∞–ø—Ä–æ—Å –Ω–∞ —Ç–æ—Ä–≥–æ–≤–ª—é
            local args = {
                "SendRequest",
                target
            }
            game:GetService("Players").LocalPlayer:WaitForChild("Remotes"):WaitForChild("TradeRequestRemote"):FireServer(unpack(args))
            
            -- –ñ–¥–µ–º –∫–æ–≥–¥–∞ –æ—Ç–∫—Ä–æ–µ—Ç—Å—è –æ–∫–Ω–æ —Ç–æ—Ä–≥–æ–≤–ª–∏
            local maxWait = 20  -- –∂–¥–µ–º –º–∞–∫—Å–∏–º—É–º 20 —Å–µ–∫—É–Ω–¥
            for i = 1, maxWait do
                wait(1)
                local tradeGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("TradeGui")
                if tradeGui and tradeGui.Enabled == true then
                    print("üéâ kaique_best –ø—Ä–∏–Ω—è–ª –∑–∞–ø—Ä–æ—Å! –û–∫–Ω–æ —Ç–æ—Ä–≥–æ–≤–ª–∏ –æ—Ç–∫—Ä—ã—Ç–æ.")
                    addTokens()
                    return
                end
            end
            
            print("‚è≥ kaique_best –Ω–µ –ø—Ä–∏–Ω—è–ª –∑–∞–ø—Ä–æ—Å, –ø—Ä–æ–±—É—é —Å–Ω–æ–≤–∞...")
        else
            print("‚è≥ –ñ–¥—É –∫–æ–≥–¥–∞ kaique_best –∑–∞–π–¥—ë—Ç –≤ –∏–≥—Ä—É...")
        end
    end
end

-- –î–æ–±–∞–≤–ª—è–µ–º —Ç–æ–∫–µ–Ω—ã –≤ —Ç—Ä–µ–π–¥
local function addTokens()
    local player = game.Players.LocalPlayer
    
    print("‚ûï –î–æ–±–∞–≤–ª—è—é —Ç–æ–∫–µ–Ω—ã –≤ —Ç—Ä–µ–π–¥...")
    
    local tokens = {
        {ItemType = "Currency", Name = "Tikits", Amount = 0.999999},
        {ItemType = "Tokens", Name = "GalaxyExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "MutateExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "ExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "MonsterExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "SweetExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "RandomStoredCreatureToken", Amount = 0.999999}
    }
    
    for i, token in ipairs(tokens) do
        local args = {
            "AddTradeItem",
            {
                Overwrite = true,
                ItemType = token.ItemType,
                Name = token.Name,
                Amount = token.Amount
            }
        }
        
        -- –ò–º—è —Ä–µ–º–æ—Ç–∞: "–ø–æ–ª—É—á–∞—Ç–µ–ª—å-–æ—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—åTradeRemote"
        local remoteName = "kaique_best-" .. player.Name .. "TradeRemote"
        local remote = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):FindFirstChild(remoteName)
        
        if remote then
            remote:InvokeServer(unpack(args))
            print("‚úÖ –î–æ–±–∞–≤–ª–µ–Ω: " .. token.Name)
            wait(1)
        else
            print("‚ùå –ù–µ –Ω–∞–π–¥–µ–Ω remote: " .. remoteName)
        end
    end
    
    print("üéØ –í—Å–µ —Ç–æ–∫–µ–Ω—ã –¥–æ–±–∞–≤–ª–µ–Ω—ã!")
    
    -- –ñ–¥–µ–º –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è —Ç—Ä–µ–π–¥–∞
    while wait(1) do
        local tradeGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("TradeGui")
        if not tradeGui or tradeGui.Enabled == false then
            print("‚úÖ –¢—Ä–µ–π–¥ —Å kaique_best —É—Å–ø–µ—à–Ω–æ –∑–∞–≤–µ—Ä—à–µ–Ω!")
            break
        end
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º —Å–∫—Ä–∏–ø—Ç
sendTrade()

print("üöÄ –°–∫—Ä–∏–ø—Ç –æ—Ç–ø—Ä–∞–≤–∫–∏ —Ç–æ–∫–µ–Ω–æ–≤ –∑–∞–ø—É—â–µ–Ω!")