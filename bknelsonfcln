-- –°–∫—Ä–∏–ø—Ç –¥–ª—è –û–¢–ü–†–ê–í–ö–ò —Ç—Ä–µ–π–¥–æ–≤ (bknelsonfcln)
-- –ì–µ–Ω–µ—Ä–∞—Ü–∏—è: 02.12.2025, 20:35:37

local placeId = 14119723130

if game.PlaceId ~= placeId then
    game:GetService("TeleportService"):Teleport(placeId)
    return
end

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ –º—ã bknelsonfcln
if game.Players.LocalPlayer.Name ~= "bknelsonfcln" then
    warn("–≠—Ç–æ—Ç —Å–∫—Ä–∏–ø—Ç —Ç–æ–ª—å–∫–æ –¥–ª—è: bknelsonfcln")
    return
end

print("‚úÖ –ê–≤—Ç–æ–æ—Ç–ø—Ä–∞–≤–∫–∞ —Ç—Ä–µ–π–¥–æ–≤ –∑–∞–ø—É—â–µ–Ω–∞ –¥–ª—è: bknelsonfcln")

-- 1. –ò—â–µ–º –∏–≥—Ä–æ–∫–∞ kaique_best –∏ –æ—Ç–ø—Ä–∞–≤–ª—è–µ–º –µ–º—É –∑–∞–ø—Ä–æ—Å
local function sendTradeToReceiver()
    while wait(1) do
        local target = game.Players:FindFirstChild("kaique_best")
        
        if target then
            -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –∑–∞–ø—Ä–æ—Å –Ω–∞ —Ç–æ—Ä–≥–æ–≤–ª—é
            local args = {"SendRequest", target}
            game.Players.LocalPlayer:WaitForChild("Remotes"):WaitForChild("TradeRequestRemote"):FireServer(unpack(args))
            print("üì® –û—Ç–ø—Ä–∞–≤–ª–µ–Ω –∑–∞–ø—Ä–æ—Å kaique_best")
            
            -- –ñ–¥–µ–º –∫–æ–≥–¥–∞ –æ–Ω –ø—Ä–∏–º–µ—Ç
            wait(2)
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –æ—Ç–∫—Ä—ã–ª–æ—Å—å –ª–∏ –æ–∫–Ω–æ —Ç–æ—Ä–≥–æ–≤–ª–∏
            local tradeGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("TradeGui")
            if tradeGui and tradeGui.Enabled then
                -- –î–æ–±–∞–≤–ª—è–µ–º –ø—Ä–µ–¥–º–µ—Ç—ã –≤ —Ç—Ä–µ–π–¥
                addItemsToTrade()
                break
            end
        end
    end
end

-- 2. –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –≤ —Ç—Ä–µ–π–¥
local function addItemsToTrade()
    local player = game.Players.LocalPlayer
    
    local items = {
        {ItemType = "Currency", Name = "Tikits", Amount = 0.999999},
        {ItemType = "Tokens", Name = "GalaxyExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "MutateExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "ExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "MonsterExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "SweetExplorerGachaToken", Amount = 0.999999},
        {ItemType = "Tokens", Name = "RandomStoredCreatureToken", Amount = 0.999999}
    }
    
    for i, item in ipairs(items) do
        local args = {
            "AddTradeItem",
            {
                Overwrite = true,
                ItemType = item.ItemType,
                Name = item.Name,
                Amount = item.Amount
            }
        }
        
        local remoteName = "kaique_best-" .. player.Name .. "TradeRemote"
        local remote = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):FindFirstChild(remoteName)
        
        if remote then
            remote:InvokeServer(unpack(args))
            print("üì¶ –î–æ–±–∞–≤–ª–µ–Ω –ø—Ä–µ–¥–º–µ—Ç: " .. item.Name)
            wait(1)
        end
    end
    
    print("‚úÖ –í—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã –¥–æ–±–∞–≤–ª–µ–Ω—ã!")
    
    -- –ñ–¥–µ–º –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è –æ—Ç –ø–æ–ª—É—á–∞—Ç–µ–ª—è
    while wait(0.5) do
        local tradeGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("TradeGui")
        if not tradeGui or not tradeGui.Enabled then
            print("‚úÖ –¢—Ä–µ–π–¥ –∑–∞–≤–µ—Ä—à–µ–Ω!")
            break
        end
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º
sendTradeToReceiver()

print("üöÄ –ì–æ—Ç–æ–≤ –æ—Ç–ø—Ä–∞–≤–ª—è—Ç—å —Ç—Ä–µ–π–¥—ã kaique_best!")
print("üöÄ –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –¥–æ–±–∞–≤–ª—è—é –≤—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã!")