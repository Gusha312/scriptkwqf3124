while true do
wait(0.1)
for d = 1,10 do
for i = 1,6 do
lol = game:GetService("Workspace").Interactions.Nodes.Food:GetChildren()[d].BillboardPart
wait(0.1)
local args = {
	lol,
	i,
	1
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("LargeNodeDropsRemote"):FireServer(unpack(args))
wait(0.1)
local args = {
	lol,
	i,
	3
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("LargeNodeDropsRemote"):FireServer(unpack(args))
wait(0.1)
local args = {
	lol,
	i,
	2
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("LargeNodeDropsRemote"):FireServer(unpack(args))
end
end
end
