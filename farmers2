-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ –Ω–∞ Discord
local function sendStatsToWebhook()
    local player = game:GetService("Players").LocalPlayer
    local webhookUrl = "https://discord.com/api/webhooks/1444659438064767158/A881Kotwv1_0efAzP3fiAG4N3kKAHrykKupJ_i7DXEwBdhO0vwazrZ8kngS2TQe3Lrkp"
    
    -- –ü–æ–ª—É—á–∞–µ–º –¥–∞–Ω–Ω—ã–µ
    local username = player.Name
    local displayName = player.DisplayName
    
    -- –ü–æ–ª—É—á–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ç–∏–∫–µ—Ç–æ–≤ –∏ –æ—á–∫–æ–≤
    local tikets = 0
    local tikitPoints = 0
    pcall(function()
        tikets = player.Data.Tikits.Value
        tikitPoints = player.Data.TikitPoints.Value
    end)
    
    -- –ü–æ–ª—É—á–∞–µ–º —Ç–æ–∫–µ–Ω—ã
    local sweetGacha = 0
    local mechaGacha = 0
    local monsterGacha = 0
    local explorerGacha = 0
    local galaxyGacha = 0
    
    pcall(function()
        sweetGacha = player.PlayerGui.Data.Items.SweetExplorerGachaToken.Value
        mechaGacha = player.PlayerGui.Data.Items.MutateExplorerGachaToken.Value
        monsterGacha = player.PlayerGui.Data.Items.MonsterExplorerGachaToken.Value
        explorerGacha = player.PlayerGui.Data.Items.ExplorerGachaToken.Value
        galaxyGacha = player.PlayerGui.Data.Items.GalaxyExplorerGachaToken.Value
    end)
    
    -- –†–∞—Å—Å—á–∏—Ç—ã–≤–∞–µ–º –≤–∞–ª—é—Ç—É (–≥—Ä–∏–±—ã)
    local mushroomCurrency = (tikets * 100) + (sweetGacha * 50) + (monsterGacha * 50) + (mechaGacha * 5) + (explorerGacha * 5) + (galaxyGacha * 5)
    
    -- –§–æ—Ä–º–∏—Ä—É–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ
    local message = string.format([[
**–°–¢–ê–¢–ò–°–¢–ò–ö–ê:**
üë§ **–ù–∏–∫ –∞–∫–∫–∞—É–Ω—Ç–∞:** %s (@%s)

üé´ **–¢–∏–∫–µ—Ç—ã:**
‚Ä¢ –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ç–∏–∫–µ—Ç–æ–≤: %d
‚Ä¢ –û—á–∫–æ–≤ –¥–ª—è —Ç–∏–∫–µ—Ç–æ–≤: %d

üéØ **–¢–æ–∫–µ–Ω—ã:**
‚Ä¢ Sweet Gacha: %d
‚Ä¢ Mecha Gacha: %d  
‚Ä¢ Monster Gacha: %d
‚Ä¢ Explorer Gacha: %d
‚Ä¢ Galaxy Gacha: %d

üí∞ **–í–∞–ª—é—Ç–∞ –∞–∫–∫–∞:** %d üçÑ (–≥—Ä–∏–±–æ–≤)
    ]], displayName, username, tikets, tikitPoints, sweetGacha, mechaGacha, monsterGacha, explorerGacha, galaxyGacha, mushroomCurrency)
    
    -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –Ω–∞ –≤–µ–±—Ö—É–∫
    pcall(function()
        local http = game:GetService("HttpService")
        local headers = {
            ["Content-Type"] = "application/json"
        }
        local data = {
            ["content"] = message
        }
        
        request({
            Url = webhookUrl,
            Method = "POST",
            Headers = headers,
            Body = http:JSONEncode(data)
        })
    end)
end

-- –í—ã–ø–æ–ª–Ω—è–µ–º –æ—Å–Ω–æ–≤–Ω–æ–π –º–µ—Ç–æ–¥
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ClaimDailyLoginRemote"):FireServer()
