-- –ü–æ–ª–Ω—ã–π —Å–∫—Ä–∏–ø—Ç: —Ç—É—Ç–æ—Ä–∏–∞–ª, –æ–±—Ä–∞–±–æ—Ç–∫–∞ —Å–º–µ—Ä—Ç–∏, –∑–∞–∫—Ä—ã–≤–∞–µ–º –º–µ—à–∞—é—â–∏–µ GUI, –Ω–∞—à–∏ –∫–ª–∏–∫–∏, —Ñ–∞—Ä–º –º–∏—Å—Å–∏–π
local VirtualInputManager = game:GetService("VirtualInputManager")
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- –ñ–¥–µ–º 5 —Å–µ–∫—É–Ω–¥ –ø–µ—Ä–µ–¥ –Ω–∞—á–∞–ª–æ–º
wait(5)

print("üéÆ –ó–∞–ø—É—Å–∫ –ø–æ–ª–Ω–æ–≥–æ —Å–∫—Ä–∏–ø—Ç–∞")

-- ========== –£–õ–£–ß–®–ï–ù–ù–´–ï –§–£–ù–ö–¶–ò–ò –î–õ–Ø –ö–õ–ò–ö–ê ==========
local function clickAtPosition(posX, posY, buttonName)
    print("üëÜ –ö–ª–∏–∫ –Ω–∞ " .. buttonName .. ": " .. math.floor(posX) .. ", " .. math.floor(posY))
    
    -- –ú–µ—Ç–æ–¥ 1: VirtualInputManager
    VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, true, game, 1)
    wait(0.05)
    VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, false, game, 1)
    
    -- –ú–µ—Ç–æ–¥ 2: –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–π –∫–ª–∏–∫ –¥–ª—è –Ω–∞–¥–µ–∂–Ω–æ—Å—Ç–∏
    wait(0.05)
    VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, true, game, 1)
    wait(0.05)
    VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, false, game, 1)
    
    return true
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –¥–≤–æ–π–Ω–æ–≥–æ –∫–ª–∏–∫–∞
local function doubleClickAtPosition(posX, posY, buttonName)
    print("üëÜüëÜ –î–≤–æ–π–Ω–æ–π –∫–ª–∏–∫ –Ω–∞ " .. buttonName .. ": " .. math.floor(posX) .. ", " .. math.floor(posY))
    
    -- –ü–µ—Ä–≤—ã–π –∫–ª–∏–∫ (–¥–≤–æ–π–Ω–æ–π –¥–ª—è –Ω–∞–¥–µ–∂–Ω–æ—Å—Ç–∏)
    for i = 1, 2 do
        VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, true, game, 1)
        wait(0.03)
        VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, false, game, 1)
        wait(0.03)
    end
    
    wait(0.3)  -- –ü–∞—É–∑–∞ –º–µ–∂–¥—É –∫–ª–∏–∫–∞–º–∏
    
    -- –í—Ç–æ—Ä–æ–π –∫–ª–∏–∫ (–¥–≤–æ–π–Ω–æ–π –¥–ª—è –Ω–∞–¥–µ–∂–Ω–æ—Å—Ç–∏)
    for i = 1, 2 do
        VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, true, game, 1)
        wait(0.03)
        VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, false, game, 1)
        wait(0.03)
    end
    
    return true
end

-- –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –∫–ª–∏–∫–∞ —á–µ—Ä–µ–∑ –∏–∑–º–µ–Ω–µ–Ω–∏–µ –ø–æ–∑–∏—Ü–∏–∏ –º—ã—à–∏
local function alternativeClick(posX, posY, buttonName)
    print("üñ±Ô∏è –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –∫–ª–∏–∫ –Ω–∞ " .. buttonName .. ": " .. math.floor(posX) .. ", " .. math.floor(posY))
    
    -- –ò–∑–º–µ–Ω—è–µ–º –ø–æ–∑–∏—Ü–∏—é –º—ã—à–∏
    VirtualInputManager:SendMouseMoveEvent(posX, posY, game)
    wait(0.1)
    
    -- –ù–∞–∂–∏–º–∞–µ–º –∏ –æ—Ç–ø—É—Å–∫–∞–µ–º –∫–Ω–æ–ø–∫—É
    VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, true, game, 1)
    wait(0.1)
    VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, false, game, 1)
    
    return true
end

-- ========== –ß–ê–°–¢–¨ 0: –û–ë–†–ê–ë–û–¢–ö–ê –°–ú–ï–†–¢–ò ==========
local function handleDeath()
    print("\n" .. string.rep("=", 50))
    print("üíÄ –ü–†–û–í–ï–†–ö–ê –°–ú–ï–†–¢–ò")
    print(string.rep("=", 50))
    
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∞–∫—Ç–∏–≤–µ–Ω –ª–∏ DeathGui
    local deathGui = playerGui:FindFirstChild("DeathGui")
    if deathGui and deathGui.Enabled == true then
        print("üíÄ DeathGui –∞–∫—Ç–∏–≤–µ–Ω! –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º —Å–º–µ—Ä—Ç—å...")
        
        -- –ò—â–µ–º –∫–Ω–æ–ø–∫—É Return
        local returnButton = deathGui:FindFirstChild("ContainerFrame")
            and deathGui.ContainerFrame:FindFirstChild("BottomFrame")
            and deathGui.ContainerFrame.BottomFrame:FindFirstChild("ButtonsFrame")
            and deathGui.ContainerFrame.BottomFrame.ButtonsFrame:FindFirstChild("Return")
        
        if returnButton then
            print("‚úÖ –ù–∞–π–¥–µ–Ω–∞ –∫–Ω–æ–ø–∫–∞ Return –≤ DeathGui")
            print("üìç –ü–æ–∑–∏—Ü–∏—è: " .. tostring(returnButton.AbsolutePosition))
            print("üìè –†–∞–∑–º–µ—Ä: " .. tostring(returnButton.AbsoluteSize))
            
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ —Å–≤–æ–π—Å—Ç–≤–∞
            local originalSize = returnButton.Size
            local originalVisible = returnButton.Visible
            
            -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–Ω–æ–ø–∫—É
            returnButton.Size = UDim2.new(5000, 5000, 5000, 5000)
            returnButton.Visible = true
            print("‚úÖ –ö–Ω–æ–ø–∫–∞ Return —É–≤–µ–ª–∏—á–µ–Ω–∞")
            
            wait(0.5)  -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
            
            -- –ö–ª–∏–∫–∞–µ–º –≤ —Ü–µ–Ω—Ç—Ä (—É–≤–µ–ª–∏—á–∏–ª–∏ –∫–Ω–æ–ø–∫—É, —Ç–µ–ø–µ—Ä—å —Ü–µ–Ω—Ç—Ä = –ø—Ä–∞–≤—ã–π –Ω–∏–∂–Ω–∏–π —É–≥–æ–ª)
            if returnButton.AbsoluteSize.X > 0 then
                local posX = returnButton.AbsolutePosition.X + (returnButton.AbsoluteSize.X / 2)
                local posY = returnButton.AbsolutePosition.Y + (returnButton.AbsoluteSize.Y / 2)
                
                print("üëÜ –ö–ª–∏–∫ –≤ —Ü–µ–Ω—Ç—Ä–µ —É–≤–µ–ª–∏—á–µ–Ω–Ω–æ–π –∫–Ω–æ–ø–∫–∏: " .. math.floor(posX) .. ", " .. math.floor(posY))
                
                -- –ü—Ä–æ–±—É–µ–º –æ–±–∞ –º–µ—Ç–æ–¥–∞ –∫–ª–∏–∫–∞
                clickAtPosition(posX, posY, "DeathGui Return Button")
                wait(0.2)
                alternativeClick(posX, posY, "DeathGui Return Button Alt")
            else
                local screenSize = workspace.CurrentCamera.ViewportSize
                clickAtPosition(screenSize.X - 50, screenSize.Y - 50, "–ü—Ä–∞–≤—ã–π –Ω–∏–∂–Ω–∏–π —É–≥–æ–ª —ç–∫—Ä–∞–Ω–∞")
            end
            
            -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Å–≤–æ–π—Å—Ç–≤–∞
            returnButton.Size = originalSize
            returnButton.Visible = originalVisible
            
            -- –ñ–¥–µ–º 1 —Å–µ–∫—É–Ω–¥—É
            wait(1)
            
            -- –£–¥–∞–ª—è–µ–º —Å–ª–æ—Ç 1
            print("üóëÔ∏è –£–¥–∞–ª—è–µ–º —Å–ª–æ—Ç 1...")
            local args = {"Slot1", false}
            local deleteSlotRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("DeleteSlotRemote")
            deleteSlotRemote:InvokeServer(unpack(args))
            
            wait(0.1)
            
            -- –°–æ–∑–¥–∞–µ–º —Å–ª–æ—Ç Sochuri
            print("‚ûï –°–æ–∑–¥–∞–µ–º —Å–ª–æ—Ç Sochuri...")
            local args = {"Sochuri"}
            local createSlotRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("CreateSlotRemote")
            createSlotRemote:InvokeServer(unpack(args))
            
            wait(0.2)
            
            print("‚úÖ –û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–º–µ—Ä—Ç–∏ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!")
            return true
        else
            print("‚ö†Ô∏è –ö–Ω–æ–ø–∫–∞ Return –Ω–µ –Ω–∞–π–¥–µ–Ω–∞ –≤ DeathGui")
        end
    else
        print("‚úÖ DeathGui –Ω–µ –∞–∫—Ç–∏–≤–µ–Ω, –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º...")
        return false
    end
    
    return false
end

-- ========== –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ü–†–û–í–ï–†–ö–ò –°–ú–ï–†–¢–ò –í–û –í–†–ï–ú–Ø –§–ê–†–ú–ê ==========
-- ========== –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ü–†–û–í–ï–†–ö–ò –°–ú–ï–†–¢–ò –í–û –í–†–ï–ú–Ø –§–ê–†–ú–ê ==========
local function checkDeathDuringFarm()
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∞–∫—Ç–∏–≤–µ–Ω –ª–∏ DeathGui
    local deathGui = playerGui:FindFirstChild("DeathGui")
    if deathGui and deathGui.Enabled == true then
        print("\nüíÄ –û–ë–ù–ê–†–£–ñ–ï–ù–ê –°–ú–ï–†–¢–¨ –í–û –í–†–ï–ú–Ø –§–ê–†–ú–ê!")
        print("‚è≥ –ñ–¥–µ–º 10 —Å–µ–∫—É–Ω–¥...")
        
        -- –ñ–¥–µ–º 10 —Å–µ–∫—É–Ω–¥
        wait(10)
        
        -- –ò—â–µ–º –∫–Ω–æ–ø–∫—É Return
        local returnButton = deathGui:FindFirstChild("ContainerFrame")
            and deathGui.ContainerFrame:FindFirstChild("BottomFrame")
            and deathGui.ContainerFrame.BottomFrame:FindFirstChild("ButtonsFrame")
            and deathGui.ContainerFrame.BottomFrame.ButtonsFrame:FindFirstChild("Return")
        
        if returnButton then
            print("‚úÖ –ù–∞–π–¥–µ–Ω–∞ –∫–Ω–æ–ø–∫–∞ Return –≤ DeathGui")
            
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ —Å–≤–æ–π—Å—Ç–≤–∞
            local originalSize = returnButton.Size
            local originalVisible = returnButton.Visible
            
            -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–Ω–æ–ø–∫—É
            returnButton.Size = UDim2.new(5000, 5000, 5000, 5000)
            returnButton.Visible = true
            print("‚úÖ –ö–Ω–æ–ø–∫–∞ Return —É–≤–µ–ª–∏—á–µ–Ω–∞")
            
            wait(0.5)  -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
            
            -- –ö–ª–∏–∫–∞–µ–º –≤ —Ü–µ–Ω—Ç—Ä
            if returnButton.AbsoluteSize.X > 0 then
                local posX = returnButton.AbsolutePosition.X + (returnButton.AbsoluteSize.X / 2)
                local posY = returnButton.AbsolutePosition.Y + (returnButton.AbsoluteSize.Y / 2)
                
                print("üëÜ –ö–ª–∏–∫ –≤ —Ü–µ–Ω—Ç—Ä–µ —É–≤–µ–ª–∏—á–µ–Ω–Ω–æ–π –∫–Ω–æ–ø–∫–∏: " .. math.floor(posX) .. ", " .. math.floor(posY))
                
                clickAtPosition(posX, posY, "DeathGui Return Button (–≤–æ –≤—Ä–µ–º—è —Ñ–∞—Ä–º–∞)")
                wait(0.2)
                alternativeClick(posX, posY, "DeathGui Return Button Alt (–≤–æ –≤—Ä–µ–º—è —Ñ–∞—Ä–º–∞)")
            else
                local screenSize = workspace.CurrentCamera.ViewportSize
                clickAtPosition(screenSize.X - 50, screenSize.Y - 50, "–ü—Ä–∞–≤—ã–π –Ω–∏–∂–Ω–∏–π —É–≥–æ–ª —ç–∫—Ä–∞–Ω–∞")
            end
            
            -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Å–≤–æ–π—Å—Ç–≤–∞
            returnButton.Size = originalSize
            returnButton.Visible = originalVisible
            
            -- –ñ–¥–µ–º 1 —Å–µ–∫—É–Ω–¥—É
            wait(1)
            
            -- –£–¥–∞–ª—è–µ–º —Å–ª–æ—Ç 1
            print("üóëÔ∏è –£–¥–∞–ª—è–µ–º —Å–ª–æ—Ç 1...")
            local args = {"Slot1", false}
            local deleteSlotRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("DeleteSlotRemote")
            deleteSlotRemote:InvokeServer(unpack(args))
            
            wait(0.1)
            
            -- –°–æ–∑–¥–∞–µ–º —Å–ª–æ—Ç Sochuri
            print("‚ûï –°–æ–∑–¥–∞–µ–º —Å–ª–æ—Ç Sochuri...")
            local args = {"Sochuri"}
            local createSlotRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("CreateSlotRemote")
            createSlotRemote:InvokeServer(unpack(args))
            
            wait(0.2)
            
            print("‚úÖ –û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–º–µ—Ä—Ç–∏ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!")
            print("üîÑ –ó–∞–ø—É—Å–∫–∞–µ–º –ø—Ä–æ—Ü–µ—Å—Å –≤—Ö–æ–¥–∞ –≤ –∏–≥—Ä—É –∑–∞–Ω–æ–≤–æ...")
            
            -- ========== –ó–ê–ü–£–°–ö–ê–ï–ú –ü–†–û–¶–ï–°–° –í–•–û–î–ê –í –ò–ì–†–£ –ó–ê–ù–û–í–û ==========
            
            -- 1. –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª (–º–æ–∂–µ—Ç –ø–æ—Ç—Ä–µ–±–æ–≤–∞—Ç—å—Å—è –µ—Å–ª–∏ –Ω–æ–≤—ã–π —Å–ª–æ—Ç)
            print("\nüîß –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª –¥–ª—è –Ω–æ–≤–æ–≥–æ —Å–ª–æ—Ç–∞...")
            local tutorialCompleted = completeTutorial()
            
            if tutorialCompleted then
                print("‚è≥ –¢—É—Ç–æ—Ä–∏–∞–ª –∑–∞–≤–µ—Ä—à–µ–Ω, –∂–¥–µ–º 2 —Å–µ–∫—É–Ω–¥—ã...")
                wait(2)
            end
            
            -- 2. –ó–∞–∫—Ä—ã–≤–∞–µ–º –º–µ—à–∞—é—â–∏–µ GUI
            print("üîß –ó–∞–∫—Ä—ã–≤–∞–µ–º –º–µ—à–∞—é—â–∏–µ GUI...")
            local closedSomething = closeAnnoyingGUIs()
            
            if closedSomething then
                print("‚è≥ –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã...")
                
                -- –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã
                local startTime = tick()
                local waitTime = 3
                
                while tick() - startTime < waitTime do
                    if hasActiveAnnoyingGUIs() then
                        print("‚ö†Ô∏è –û–±–Ω–∞—Ä—É–∂–µ–Ω—ã –Ω–æ–≤—ã–µ –º–µ—à–∞—é—â–∏–µ GUI, –∑–∞–∫—Ä—ã–≤–∞–µ–º...")
                        closeAnnoyingGUIs()
                        startTime = tick() -- –°–±—Ä–∞—Å—ã–≤–∞–µ–º —Ç–∞–π–º–µ—Ä
                    end
                    wait(0.5)
                end
                
                print("‚úÖ –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ –º–µ—à–∞—é—â–∏—Ö GUI –Ω–µ—Ç...")
                
                if not hasActiveAnnoyingGUIs() then
                    print("üéâ –ù–µ—Ç –º–µ—à–∞—é—â–∏—Ö GUI, –≤—ã–ø–æ–ª–Ω—è–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏!")
                    doOurClicks()
                else
                    print("‚ùå –í—Å–µ –µ—â–µ –µ—Å—Ç—å –º–µ—à–∞—é—â–∏–µ GUI, –ø—Ä–æ–ø—É—Å–∫–∞–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏")
                end
            else
                print("‚úÖ –ù–µ—Ç –º–µ—à–∞—é—â–∏—Ö GUI, —Å—Ä–∞–∑—É –≤—ã–ø–æ–ª–Ω—è–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏!")
                doOurClicks()
            end
            
            -- 3. –ñ–¥–µ–º –∏ –≤–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ —Ñ–∞—Ä–º—É
            print("\n" .. string.rep("=", 50))
            print("‚è≥ –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ —Ñ–∞—Ä–º—É –º–∏—Å—Å–∏–π —á–µ—Ä–µ–∑ 2 —Å–µ–∫—É–Ω–¥—ã...")
            print(string.rep("=", 50))
            wait(2)
            
            -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º true —á—Ç–æ–±—ã —Å–æ–æ–±—â–∏—Ç—å —á—Ç–æ —Å–º–µ—Ä—Ç—å –æ–±—Ä–∞–±–æ—Ç–∞–Ω–∞ –∏ –Ω—É–∂–Ω–æ –≤–µ—Ä–Ω—É—Ç—å—Å—è –∫ —Ñ–∞—Ä–º—É
            return true
            
        else
            print("‚ö†Ô∏è –ö–Ω–æ–ø–∫–∞ Return –Ω–µ –Ω–∞–π–¥–µ–Ω–∞ –≤ DeathGui")
        end
    end
    
    return false
end

-- ========== –ß–ê–°–¢–¨ 0.5: –û–ë–†–ê–ë–û–¢–ö–ê –¢–£–¢–û–†–ò–ê–õ–ê ==========
local function completeTutorial()
    print("\n" .. string.rep("=", 50))
    print("üìö –ü–†–û–í–ï–†–ö–ê –¢–£–¢–û–†–ò–ê–õ–ê")
    print(string.rep("=", 50))
    
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∑–∞–≤–µ—Ä—à–µ–Ω –ª–∏ —Ç—É—Ç–æ—Ä–∏–∞–ª
    local tutorialCompleted = true
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –ø—É—Ç—å: game:GetService("Players").localplayer.PlayerGui.Data.Tutorial.TutorialCompletedAt.Value
    if playerGui:FindFirstChild("Data") then
        local data = playerGui.Data
        if data:FindFirstChild("Tutorial") then
            local tutorial = data.Tutorial
            if tutorial:FindFirstChild("TutorialCompletedAt") then
                local value = tutorial.TutorialCompletedAt.Value
                print("üìä TutorialCompletedAt.Value = " .. tostring(value))
                
                -- –ï—Å–ª–∏ –∑–Ω–∞—á–µ–Ω–∏–µ = 0, —Ç—É—Ç–æ—Ä–∏–∞–ª –Ω–µ –∑–∞–≤–µ—Ä—à–µ–Ω
                if value == 0 then
                    tutorialCompleted = false
                    print("üìö –¢—É—Ç–æ—Ä–∏–∞–ª –ù–ï –∑–∞–≤–µ—Ä—à–µ–Ω (Value = 0)")
                else
                    print("üìö –¢—É—Ç–æ—Ä–∏–∞–ª –∑–∞–≤–µ—Ä—à–µ–Ω (Value > 0)")
                end
            else
                print("‚ö†Ô∏è TutorialCompletedAt –Ω–µ –Ω–∞–π–¥–µ–Ω")
                tutorialCompleted = false
            end
        else
            print("‚ö†Ô∏è Tutorial –Ω–µ –Ω–∞–π–¥–µ–Ω")
            tutorialCompleted = false
        end
    else
        print("‚ö†Ô∏è Data –Ω–µ –Ω–∞–π–¥–µ–Ω")
        tutorialCompleted = false
    end
    
    -- –ï—Å–ª–∏ —Ç—É—Ç–æ—Ä–∏–∞–ª –Ω–µ –∑–∞–≤–µ—Ä—à–µ–Ω (Value = 0), –≤—ã–ø–æ–ª–Ω—è–µ–º –µ–≥–æ
    if not tutorialCompleted then
        print("\nüéØ –¢–£–¢–û–†–ò–ê–õ –ù–ï –ó–ê–í–ï–†–®–ï–ù! –í–´–ü–û–õ–ù–Ø–ï–ú...")
        
        -- –í–∫–ª—é—á–∞–µ–º TutorialGui –µ—Å–ª–∏ –æ–Ω –æ—Ç–∫–ª—é—á–µ–Ω
        local tutorialGui = playerGui:FindFirstChild("TutorialGui")
        if tutorialGui then
            if not tutorialGui.Enabled then
                print("üîß –í–∫–ª—é—á–∞–µ–º TutorialGui...")
                tutorialGui.Enabled = true
                wait(1)
            end
        else
            print("‚ö†Ô∏è TutorialGui –Ω–µ –Ω–∞–π–¥–µ–Ω")
            return false
        end
        
        -- 1. –ü–µ—Ä–≤–∞—è –∫–Ω–æ–ø–∫–∞: UpperLabel (–î–í–û–ô–ù–û–ô –ö–õ–ò–ö)
        print("\n1. –ò—â–µ–º –∏ –∫–ª–∏–∫–∞–µ–º –Ω–∞ UpperLabel (–î–í–û–ô–ù–û–ô –ö–õ–ò–ö)...")
        
        -- –¢–æ—á–Ω—ã–π –ø—É—Ç—å: game:GetService("Players").wrerwerrewer.PlayerGui.TutorialGui.SelectionFrame.SlotsFrame.Sochuri.InnerFrame.SelectButton.UpperLabel
        local upperLabel = nil
        
        -- –ü–æ–∏—Å–∫ –ø–æ —Ç–æ—á–Ω–æ–º—É –ø—É—Ç–∏
        if tutorialGui then
            local selectionFrame = tutorialGui:FindFirstChild("SelectionFrame")
            if selectionFrame then
                local slotsFrame = selectionFrame:FindFirstChild("SlotsFrame")
                if slotsFrame then
                    local sochuri = slotsFrame:FindFirstChild("Sochuri")
                    if sochuri then
                        local innerFrame = sochuri:FindFirstChild("InnerFrame")
                        if innerFrame then
                            local selectButton = innerFrame:FindFirstChild("SelectButton")
                            if selectButton then
                                upperLabel = selectButton:FindFirstChild("UpperLabel")
                                if upperLabel then
                                    print("‚úÖ –ù–∞–π–¥–µ–Ω UpperLabel –ø–æ —Ç–æ—á–Ω–æ–º—É –ø—É—Ç–∏")
                                    print("üìç –ü–æ–∑–∏—Ü–∏—è: " .. tostring(upperLabel.AbsolutePosition))
                                    print("üìè –†–∞–∑–º–µ—Ä: " .. tostring(upperLabel.AbsoluteSize))
                                end
                            end
                        end
                    end
                end
            end
        end
        
        if upperLabel then
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–π —Ä–∞–∑–º–µ—Ä
            local originalSize = upperLabel.Size
            local originalVisible = upperLabel.Visible
            
            -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º
            upperLabel.Size = UDim2.new(5000, 5000, 5000, 5000)
            upperLabel.Visible = true
            print("‚úÖ UpperLabel —É–≤–µ–ª–∏—á–µ–Ω –¥–æ 5000,5000,5000,5000")
            
            wait(0.5)  -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
            
            -- –ö–ª–∏–∫–∞–µ–º –≤ –¶–ï–ù–¢–† —É–≤–µ–ª–∏—á–µ–Ω–Ω–æ–π –∫–Ω–æ–ø–∫–∏
            if upperLabel.AbsoluteSize.X > 0 and upperLabel.AbsoluteSize.Y > 0 then
                local posX = upperLabel.AbsolutePosition.X + (upperLabel.AbsoluteSize.X / 2)
                local posY = upperLabel.AbsolutePosition.Y + (upperLabel.AbsoluteSize.Y / 2)
                
                print("üëÜ –ö–ª–∏–∫ –≤ —Ü–µ–Ω—Ç—Ä–µ: " .. math.floor(posX) .. ", " .. math.floor(posY))
                
                -- –î–≤–æ–π–Ω–æ–π –∫–ª–∏–∫
                doubleClickAtPosition(posX, posY, "UpperLabel")
                
                -- –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –º–µ—Ç–æ–¥
                wait(0.2)
                alternativeClick(posX, posY, "UpperLabel Alt")
            else
                local screenSize = workspace.CurrentCamera.ViewportSize
                doubleClickAtPosition(screenSize.X / 2, screenSize.Y / 2, "–¶–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
            end
            
            -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —Ä–∞–∑–º–µ—Ä
            upperLabel.Size = originalSize
            upperLabel.Visible = originalVisible
            print("‚úÖ UpperLabel –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
        else
            print("‚ö†Ô∏è UpperLabel –Ω–µ –Ω–∞–π–¥–µ–Ω –ø–æ —Ç–æ—á–Ω–æ–º—É –ø—É—Ç–∏")
        end
        
        -- –ñ–¥–µ–º 2 —Å–µ–∫—É–Ω–¥—ã –ø–µ—Ä–µ–¥ –≤—Ç–æ—Ä–æ–π –∫–Ω–æ–ø–∫–æ–π
        print("\n‚è≥ –ñ–¥–µ–º 2 —Å–µ–∫—É–Ω–¥—ã –ø–µ—Ä–µ–¥ –≤—Ç–æ—Ä–æ–π –∫–Ω–æ–ø–∫–æ–π...")
        wait(2)
        
        -- 2. –í—Ç–æ—Ä–∞—è –∫–Ω–æ–ø–∫–∞: PlayButton –≤ EditorGui (–î–í–û–ô–ù–û–ô –ö–õ–ò–ö)
        print("\n2. –ò—â–µ–º –∏ –∫–ª–∏–∫–∞–µ–º –Ω–∞ PlayButton –≤ EditorGui (–î–í–û–ô–ù–û–ô –ö–õ–ò–ö)...")
        
        -- –í–∫–ª—é—á–∞–µ–º EditorGui –µ—Å–ª–∏ –æ–Ω –æ—Ç–∫–ª—é—á–µ–Ω
        local editorGui = playerGui:FindFirstChild("EditorGui")
        if editorGui then
            if not editorGui.Enabled then
                print("üîß –í–∫–ª—é—á–∞–µ–º EditorGui...")
                editorGui.Enabled = true
                wait(1)
            end
        else
            print("‚ö†Ô∏è EditorGui –Ω–µ –Ω–∞–π–¥–µ–Ω")
        end
        
        -- –¢–æ—á–Ω—ã–π –ø—É—Ç—å: game:GetService("Players").wrerwerrewer.PlayerGui.EditorGui.EditFrame.BottomFrame.ButtonsFrame.PlayButton
        local playButton = nil
        
        -- –ü–æ–∏—Å–∫ –ø–æ —Ç–æ—á–Ω–æ–º—É –ø—É—Ç–∏
        if editorGui then
            local editFrame = editorGui:FindFirstChild("EditFrame")
            if editFrame then
                local bottomFrame = editFrame:FindFirstChild("BottomFrame")
                if bottomFrame then
                    local buttonsFrame = bottomFrame:FindFirstChild("ButtonsFrame")
                    if buttonsFrame then
                        playButton = buttonsFrame:FindFirstChild("PlayButton")
                        if playButton then
                            print("‚úÖ –ù–∞–π–¥–µ–Ω PlayButton –ø–æ —Ç–æ—á–Ω–æ–º—É –ø—É—Ç–∏")
                            print("üìç –ü–æ–∑–∏—Ü–∏—è: " .. tostring(playButton.AbsolutePosition))
                            print("üìè –†–∞–∑–º–µ—Ä: " .. tostring(playButton.AbsoluteSize))
                        end
                    end
                end
            end
        end
        
        if playButton then
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–π —Ä–∞–∑–º–µ—Ä
            local originalSize = playButton.Size
            local originalVisible = playButton.Visible
            
            -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º
            playButton.Size = UDim2.new(5000, 5000, 5000, 5000)
            playButton.Visible = true
            print("‚úÖ PlayButton —É–≤–µ–ª–∏—á–µ–Ω –¥–æ 5000,5000,5000,5000")
            
            wait(0.5)  -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
            
            -- –ö–ª–∏–∫–∞–µ–º –≤ –¶–ï–ù–¢–† —É–≤–µ–ª–∏—á–µ–Ω–Ω–æ–π –∫–Ω–æ–ø–∫–∏
            if playButton.AbsoluteSize.X > 0 and playButton.AbsoluteSize.Y > 0 then
                local posX = playButton.AbsolutePosition.X + (playButton.AbsoluteSize.X / 2)
                local posY = playButton.AbsolutePosition.Y + (playButton.AbsoluteSize.Y / 2)
                
                print("üëÜ –ö–ª–∏–∫ –≤ —Ü–µ–Ω—Ç—Ä–µ: " .. math.floor(posX) .. ", " .. math.floor(posY))
                
                -- –î–≤–æ–π–Ω–æ–π –∫–ª–∏–∫
                doubleClickAtPosition(posX, posY, "PlayButton")
                
                -- –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –º–µ—Ç–æ–¥
                wait(0.2)
                alternativeClick(posX, posY, "PlayButton Alt")
            else
                local screenSize = workspace.CurrentCamera.ViewportSize
                doubleClickAtPosition(screenSize.X / 2, screenSize.Y / 2, "–¶–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
            end
            
            -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —Ä–∞–∑–º–µ—Ä
            playButton.Size = originalSize
            playButton.Visible = originalVisible
            print("‚úÖ PlayButton –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
        else
            print("‚ö†Ô∏è PlayButton –Ω–µ –Ω–∞–π–¥–µ–Ω –ø–æ —Ç–æ—á–Ω–æ–º—É –ø—É—Ç–∏")
        end
        
        wait(2)
        
        -- 3. –í—ã–ø–æ–ª–Ω—è–µ–º —ç—Ç–∞–ø—ã —Ç—É—Ç–æ—Ä–∏–∞–ª–∞ —á–µ—Ä–µ–∑ Remote
        print("\n3. –í—ã–ø–æ–ª–Ω—è–µ–º —ç—Ç–∞–ø—ã —Ç—É—Ç–æ—Ä–∏–∞–ª–∞...")
        
        -- SniffWater
        print("üíß –≠—Ç–∞–ø SniffWater...")
        local sniffWaterArgs = {"SniffWater"}
        local postTutorialRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("PostTutorialStageRemote")
        if postTutorialRemote then
            postTutorialRemote:FireServer(unpack(sniffWaterArgs))
            print("‚úÖ SniffWater –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        else
            print("‚ö†Ô∏è PostTutorialStageRemote –Ω–µ –Ω–∞–π–¥–µ–Ω")
        end
        
        wait(0.5)
        
        -- SniffFood
        print("üçé –≠—Ç–∞–ø SniffFood...")
        local sniffFoodArgs = {"SniffFood"}
        if postTutorialRemote then
            postTutorialRemote:FireServer(unpack(sniffFoodArgs))
            print("‚úÖ SniffFood –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        end
        
        wait(0.5)
        
        -- Complete
        print("‚úÖ –ó–∞–≤–µ—Ä—à–∞–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª...")
        local completeArgs = {"Complete"}
        local setTutorialRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("SetTutorialStageRemote")
        if setTutorialRemote then
            setTutorialRemote:InvokeServer(unpack(completeArgs))
            print("‚úÖ Complete –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        else
            print("‚ö†Ô∏è SetTutorialStageRemote –Ω–µ –Ω–∞–π–¥–µ–Ω")
        end
        
        -- –û—Ç–∫–ª—é—á–∞–µ–º GUI –ø–æ—Å–ª–µ –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è —Ç—É—Ç–æ—Ä–∏–∞–ª–∞
        if tutorialGui then
            tutorialGui.Enabled = false
            print("üö´ –û—Ç–∫–ª—é—á–∞–µ–º TutorialGui –ø–æ—Å–ª–µ —Ç—É—Ç–æ—Ä–∏–∞–ª–∞")
        end
        
        if editorGui then
            editorGui.Enabled = false
            print("üö´ –û—Ç–∫–ª—é—á–∞–µ–º EditorGui –ø–æ—Å–ª–µ —Ç—É—Ç–æ—Ä–∏–∞–ª–∞")
        end
        
        print("\nüéâ –¢–£–¢–û–†–ò–ê–õ –í–´–ü–û–õ–ù–ï–ù!")
        
        -- –ñ–¥–µ–º 1 —Å–µ–∫—É–Ω–¥—É –ø–µ—Ä–µ–¥ –ø—Ä–æ–¥–æ–ª–∂–µ–Ω–∏–µ–º
        wait(1)
        
        return true
    else
        print("‚úÖ –¢—É—Ç–æ—Ä–∏–∞–ª —É–∂–µ –∑–∞–≤–µ—Ä—à–µ–Ω (Value > 0), –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º...")
        return false
    end
end

-- ========== –ß–ê–°–¢–¨ 1: –ó–∞–∫—Ä—ã–≤–∞–µ–º –º–µ—à–∞—é—â–∏–µ GUI (–ë–ï–ó TutorialGui) ==========
local function closeAnnoyingGUIs()
    local targetPlayer = game:GetService("Players").LocalPlayer
    if not targetPlayer then
        print("‚ö†Ô∏è –õ–æ–∫–∞–ª—å–Ω—ã–π –∏–≥—Ä–æ–∫ –Ω–µ –Ω–∞–π–¥–µ–Ω")
        return false
    end
    
    local playerGui = targetPlayer.PlayerGui
    local closedCount = 0
    
    -- –°–ø–∏—Å–æ–∫ GUI –∫–æ—Ç–æ—Ä—ã–µ –º–µ—à–∞—é—Ç (–£–ë–†–ê–õ–ò TutorialGui –∏–∑ —Å–ø–∏—Å–∫–∞!)
    local annoyingGUIs = {
        "PromptGui", 
        "UpdateLogGui",
        "DailyLoginGui",
        "AdventCalendarGui"
        -- TutorialGui –ù–ï –æ—Ç–∫–ª—é—á–∞–µ–º, —Ç–∞–∫ –∫–∞–∫ –æ–Ω –Ω—É–∂–µ–Ω –¥–ª—è —Ç—É—Ç–æ—Ä–∏–∞–ª–∞
    }
    
    -- –ó–∞–∫—Ä—ã–≤–∞–µ–º –≤—Å–µ –º–µ—à–∞—é—â–∏–µ GUI
    for _, guiName in ipairs(annoyingGUIs) do
        local gui = playerGui:FindFirstChild(guiName)
        if gui and gui:IsA("ScreenGui") and gui.Enabled == true then
            gui.Enabled = false
            closedCount = closedCount + 1
            print("üö´ –û—Ç–∫–ª—é—á–µ–Ω: " .. guiName)
        end
    end
    
    return closedCount > 0
end

local function hasActiveAnnoyingGUIs()
    local targetPlayer = game.Players.LocalPlayer
    if not targetPlayer then return false end
    
    local playerGui = targetPlayer.PlayerGui
    -- –£–ë–†–ê–õ–ò TutorialGui –∏–∑ –ø—Ä–æ–≤–µ—Ä–∫–∏
    local annoyingGUIs = {"PromptGui", "UpdateLogGui", "DailyLoginGui", "AdventCalendarGui"}
    
    for _, guiName in ipairs(annoyingGUIs) do
        local gui = playerGui:FindFirstChild(guiName)
        if gui and gui:IsA("ScreenGui") and gui.Enabled == true then
            return true
        end
    end
    
    return false
end

-- ========== –ß–ê–°–¢–¨ 2: –ù–∞—à–∏ –∫–ª–∏–∫–∏ ==========
local function doOurClicks()
    print("üéØ –í—ã–ø–æ–ª–Ω—è–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏...")
    
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    -- 1. –ü–µ—Ä–≤—ã–π —ç–ª–µ–º–µ–Ω—Ç
    print("\nüéØ –ü–µ—Ä–≤—ã–π —ç–ª–µ–º–µ–Ω—Ç...")
    local frame1 = playerGui:FindFirstChild("SaveSelectionGui") 
        and playerGui.SaveSelectionGui:FindFirstChild("ContainerFrame")
        and playerGui.SaveSelectionGui.ContainerFrame:FindFirstChild("AllSlotsFrame")
        and playerGui.SaveSelectionGui.ContainerFrame.AllSlotsFrame:FindFirstChild("PagesFrame")
        and playerGui.SaveSelectionGui.ContainerFrame.AllSlotsFrame.PagesFrame:FindFirstChild("1")
    
    if frame1 then
        -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º
        frame1.Size = UDim2.new(5000, 5000, 5000, 5000)
        print("‚úÖ –ü–µ—Ä–≤—ã–π —ç–ª–µ–º–µ–Ω—Ç —É–≤–µ–ª–∏—á–µ–Ω")
        
        wait(0.5)  -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
        
        -- –ö–ª–∏–∫–∞–µ–º –≤ –¶–ï–ù–¢–† —É–≤–µ–ª–∏—á–µ–Ω–Ω–æ–≥–æ —ç–ª–µ–º–µ–Ω—Ç–∞
        if frame1.AbsoluteSize.X > 0 then
            local posX = frame1.AbsolutePosition.X + (frame1.AbsoluteSize.X / 2)
            local posY = frame1.AbsolutePosition.Y + (frame1.AbsoluteSize.Y / 2)
            
            print("üëÜ –ö–ª–∏–∫ –≤ —Ü–µ–Ω—Ç—Ä–µ: " .. math.floor(posX) .. ", " .. math.floor(posY))
            clickAtPosition(posX, posY, "frame1")
            wait(0.2)
            alternativeClick(posX, posY, "frame1 Alt")
        else
            local screenSize = workspace.CurrentCamera.ViewportSize
            clickAtPosition(screenSize.X/2, screenSize.Y/2, "–¶–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞ (frame1)")
        end
        
        -- –£–º–µ–Ω—å—à–∞–µ–º
        frame1.Size = UDim2.new(0, 0, 0, 0)
        print("‚úÖ –ü–µ—Ä–≤—ã–π —ç–ª–µ–º–µ–Ω—Ç —É–º–µ–Ω—å—à–µ–Ω")
    else
        print("‚ö†Ô∏è –ü–µ—Ä–≤—ã–π —ç–ª–µ–º–µ–Ω—Ç –Ω–µ –Ω–∞–π–¥–µ–Ω")
    end
    
    wait(1)
    
    -- 2. –í—Ç–æ—Ä–∞—è –∫–Ω–æ–ø–∫–∞ (PlayButton) - –î–í–û–ô–ù–û–ô –ö–õ–ò–ö
    print("\nüéØ –í—Ç–æ—Ä–∞—è –∫–Ω–æ–ø–∫–∞ (PlayButton) - –î–í–û–ô–ù–û–ô –ö–õ–ò–ö...")
    
    -- –ò—â–µ–º PlayButton
    local button2 = playerGui:FindFirstChild("SaveSelectionGui") 
        and playerGui.SaveSelectionGui:FindFirstChild("ContainerFrame")
        and playerGui.SaveSelectionGui.ContainerFrame:FindFirstChild("AllSlotsFrame")
        and playerGui.SaveSelectionGui.ContainerFrame.AllSlotsFrame:FindFirstChild("SlotsFrame")
        and playerGui.SaveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame:FindFirstChild("1")
        and playerGui.SaveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame["1"]:FindFirstChild("InnerFrame")
        and playerGui.SaveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame["1"].InnerFrame:FindFirstChild("CreatureFrame")
        and playerGui.SaveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame["1"].InnerFrame.CreatureFrame:FindFirstChild("ButtonsFrame")
        and playerGui.SaveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame["1"].InnerFrame.CreatureFrame.ButtonsFrame:FindFirstChild("PlayButton")
    
    if button2 then
        print("‚úÖ –ö–Ω–æ–ø–∫–∞ PlayButton –Ω–∞–π–¥–µ–Ω–∞")
        
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ —Å–≤–æ–π—Å—Ç–≤–∞
        local originalSize = button2.Size
        
        -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–Ω–æ–ø–∫—É
        button2.Size = UDim2.new(5000, 5000, 5000, 5000)
        print("‚úÖ –ö–Ω–æ–ø–∫–∞ —É–≤–µ–ª–∏—á–µ–Ω–∞")
        
        wait(0.5)  -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
        
        -- –î–í–û–ô–ù–û–ô –ö–õ–ò–ö –Ω–∞ PlayButton (–≤ –¶–ï–ù–¢–†)
        if button2.AbsoluteSize.X > 0 and button2.AbsoluteSize.Y > 0 then
            -- –í—ã—á–∏—Å–ª—è–µ–º –ø–æ–∑–∏—Ü–∏—é –∫–ª–∏–∫–∞ (—Ü–µ–Ω—Ç—Ä)
            local posX = button2.AbsolutePosition.X + (button2.AbsoluteSize.X / 2)
            local posY = button2.AbsolutePosition.Y + (button2.AbsoluteSize.Y / 2)
            
            print("üëÜüëÜ –î–≤–æ–π–Ω–æ–π –∫–ª–∏–∫ –≤ —Ü–µ–Ω—Ç—Ä–µ: " .. math.floor(posX) .. ", " .. math.floor(posY))
            
            doubleClickAtPosition(posX, posY, "PlayButton")
            wait(0.2)
            alternativeClick(posX, posY, "PlayButton Alt")
            
        else
            print("‚ö†Ô∏è –ù–µ–∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–π —Ä–∞–∑–º–µ—Ä –∫–Ω–æ–ø–∫–∏, –∫–ª–∏–∫–∞–µ–º –ø–æ —Ü–µ–Ω—Ç—Ä—É")
            local screenSize = workspace.CurrentCamera.ViewportSize
            
            doubleClickAtPosition(screenSize.X/2, screenSize.Y/2, "–¶–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞ (PlayButton)")
        end
        
        -- –£–º–µ–Ω—å—à–∞–µ–º –∫–Ω–æ–ø–∫—É
        button2.Size = originalSize
        print("‚úÖ –ö–Ω–æ–ø–∫–∞ —É–º–µ–Ω—å—à–µ–Ω–∞")
    else
        print("‚ö†Ô∏è –ö–Ω–æ–ø–∫–∞ PlayButton –Ω–µ –Ω–∞–π–¥–µ–Ω–∞")
    end
    
    return true
end

-- ========== –ß–ê–°–¢–¨ 3: –§–ê–†–ú –ú–ò–°–°–ò–ô ==========
local function startMissionFarm()
    print("\n" .. string.rep("=", 50))
    print("üöÄ –ó–ê–ü–£–°–ö–ê–ï–ú –§–ê–†–ú –ú–ò–°–°–ò–ô")
    print(string.rep("=", 50) .. "\n")
    
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local targetPlayer = game.Players.LocalPlayer
    
    -- –ñ–¥–µ–º –∑–∞–≥—Ä—É–∑–∫–∏ –¥–∞–Ω–Ω—ã—Ö
    print("‚è≥ –û–∂–∏–¥–∞–µ–º –∑–∞–≥—Ä—É–∑–∫–∏ –¥–∞–Ω–Ω—ã—Ö...")
    local playerGui = targetPlayer:WaitForChild("PlayerGui")
    local data = playerGui:WaitForChild("Data")
    local missions = data:WaitForChild("Missions")
    local regionMissions = missions:WaitForChild("RegionMissions")
    
    print("‚úÖ –î–∞–Ω–Ω—ã–µ –∑–∞–≥—Ä—É–∂–µ–Ω—ã")
    
    -- –ñ–¥–µ–º —Ä–µ–º–æ—É—Ç—ã
    local ChangeRegionRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("ChangeRegionRemote")
    local SetMissionRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("SetMissionRemote")
    local MudRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Mud")
    local FoodRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Food")
    local DrinkRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("DrinkRemote")
    local NPCDamageRemoteBreath = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("NPCDamageRemoteBreath")
    
    -- –ö–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
    local teleportPositions = {
        Vector3.new(2418.49072, 332.279358, -118.064125),
        Vector3.new(2518.49072, 332.279358, -118.064125),
        Vector3.new(2318.49072, 332.279358, -118.064125)
    }
    
    local concealScentPosition = Vector3.new(2434.90479, 277.143066, -1289.48108)
    
    -- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ
    local lastFoodDrinkTime = 0
    local lastAttackTime = 0
    local foodItems = {"Grass", "Fruit", "Ribs", "NPC Carcass"}
    local currentFoodIndex = 1
    local lastDeathCheck = 0
    
    -- –°–ø–∏—Å–æ–∫ –º–∏—Å—Å–∏–π –∫–æ—Ç–æ—Ä—ã–µ –Ω—É–∂–Ω–æ –∏–≥–Ω–æ—Ä–∏—Ä–æ–≤–∞—Ç—å
    local ignoreMissions = {
        ShoomPilesCollected = true
    }
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –Ω–µ–≤—ã–ø–æ–ª–Ω–µ–Ω–Ω—ã—Ö –º–∏—Å—Å–∏–π
    function getUncompletedMissions()
        local uncompleted = {}
        
        for _, region in pairs(regionMissions:GetChildren()) do
            for _, mission in pairs(region:GetChildren()) do
                if mission:IsA("BoolValue") and not mission.Value then
                    if not ignoreMissions[mission.Name] then
                        table.insert(uncompleted, {
                            regionName = region.Name,
                            missionName = mission.Name,
                            missionObject = mission
                        })
                    else
                        print("‚è≠Ô∏è –ü—Ä–æ–ø—É—Å–∫–∞–µ–º –º–∏—Å—Å–∏—é: " .. region.Name .. "." .. mission.Name)
                    end
                end
            end
        end
        
        return uncompleted
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–º–µ–Ω—ã —Ä–µ–≥–∏–æ–Ω–∞
    function changeRegion(regionName)
        print("üìç –ú–µ–Ω—è–µ–º —Ä–µ–≥–∏–æ–Ω –Ω–∞: " .. regionName)
        local args = { regionName }
        ChangeRegionRemote:FireServer(unpack(args))
        wait(1)
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –∏–≥—Ä–æ–∫–∞
    function teleportPlayer(position)
        local character = targetPlayer.Character
        if character then
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                humanoidRootPart.CFrame = CFrame.new(position)
                print("üìç –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–ª–∏—Å—å –≤: " .. tostring(position))
            end
        end
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –µ–¥—ã –∏ –≤–æ–¥—ã
    function useFoodAndWater()
        local currentTime = tick()
        if currentTime - lastFoodDrinkTime >= 2 then
            -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –µ–¥—É (–ø—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ –≤–∞—Ä–∏–∞–Ω—Ç—ã)
            local foodTypes = {"NPC Carcass", "Grass", "Fruit", "Ribs", "Plant"}
            
            for _, foodName in ipairs(foodTypes) do
                local food = workspace.Interactions.Food:FindFirstChild(foodName)
                if food then
                    local foodArgs = { food }
                    FoodRemote:FireServer(unpack(foodArgs))
                    print("üçé –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∞ –µ–¥–∞: " .. foodName)
                    break
                end
            end
            
            -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –≤–æ–¥—É
            local lake = workspace.Interactions.Lakes:FindFirstChild("Lake")
            if lake then
                local drinkArgs = { lake }
                DrinkRemote:FireServer(unpack(drinkArgs))
                print("üíß –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∞ –≤–æ–¥–∞")
            else
                -- –ü—Ä–æ–±—É–µ–º –Ω–∞–π—Ç–∏ –ª—é–±–æ–π –∏—Å—Ç–æ—á–Ω–∏–∫ –≤–æ–¥—ã
                local lakesFolder = workspace.Interactions.Lakes
                if lakesFolder then
                    for _, waterItem in pairs(lakesFolder:GetChildren()) do
                        if waterItem:IsA("BasePart") or waterItem:IsA("MeshPart") then
                            local drinkArgs = { waterItem }
                            DrinkRemote:FireServer(unpack(drinkArgs))
                            print("üíß –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω –∏—Å—Ç–æ—á–Ω–∏–∫ –≤–æ–¥—ã: " .. waterItem.Name)
                            break
                        end
                    end
                end
            end
            
            lastFoodDrinkTime = currentTime
        end
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –±–ª–∏–∂–∞–π—à–µ–≥–æ Mud –æ–±—ä–µ–∫—Ç–∞
    function findNearestMud()
        local character = targetPlayer.Character
        if not character then return nil end
        
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if not humanoidRootPart then return nil end
        
        local playerPos = humanoidRootPart.Position
        local nearestMud = nil
        local nearestDistance = math.huge
        
        local mudFolder = workspace.Interactions.Mud
        if mudFolder then
            for _, mudObject in pairs(mudFolder:GetChildren()) do
                if mudObject:IsA("Part") or mudObject:IsA("MeshPart") then
                    local distance = (playerPos - mudObject.Position).Magnitude
                    if distance < nearestDistance then
                        nearestDistance = distance
                        nearestMud = mudObject
                    end
                end
            end
        end
        
        return nearestMud
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ NPC –¥–ª—è –∞—Ç–∞–∫–∏
    function findNPCForAttack()
        local rootsNPC = workspace.Interactions.NPCs:FindFirstChild("Roots")
        if rootsNPC then
            local rootPart = rootsNPC:FindFirstChild("RootPart")
            if rootPart then
                return rootPart
            end
        end
        
        for _, npc in pairs(workspace.Interactions.NPCs:GetChildren()) do
            local rootPart = npc:FindFirstChild("RootPart") or npc:FindFirstChild("HumanoidRootPart")
            if rootPart then
                return rootPart
            end
        end
        
        return nil
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –∫ NPC —Å–ø–∞–≤–Ω—É
    function teleportToRandomNPCSpawn()
        local npcSpawns = workspace.Interactions.NPCSpawns:GetChildren()
        if #npcSpawns > 0 then
            local randomSpawn = npcSpawns[math.random(1, #npcSpawns)]
            if randomSpawn then
                local spawnPosition = randomSpawn.Position + Vector3.new(0, 30, 0)
                teleportPlayer(spawnPosition)
                return true
            end
        end
        return false
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞—Ç–∞–∫–∏ NPC
    function attackNPC()
        local currentTime = tick()
        if currentTime - lastAttackTime >= 0.5 then
            local targetNPC = findNPCForAttack()
            if targetNPC then
                local attackArgs = { { targetNPC } }
                NPCDamageRemoteBreath:FireServer(unpack(attackArgs))
                lastAttackTime = currentTime
                print("‚öîÔ∏è –ê—Ç–∞–∫–æ–≤–∞–Ω NPC: " .. targetNPC.Parent.Name)
                return true
            else
                print("‚ö†Ô∏è NPC –¥–ª—è –∞—Ç–∞–∫–∏ –Ω–µ –Ω–∞–π–¥–µ–Ω")
            end
        end
        return false
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ Sniff
    function completeSniffMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º Sniff –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 40 do
            attempts = attempts + 1
            
            useFoodAndWater()
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
            local currentTime = tick()
            if currentTime - lastDeathCheck >= 10 then
                if checkDeathDuringFarm() then
                    print("üîÑ –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—é –º–∏—Å—Å–∏–∏ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏...")
                    wait(2)
                end
                lastDeathCheck = currentTime
            end
            
            local missionArgs = { 1 }
            SetMissionRemote:FireServer(unpack(missionArgs))
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local sniffMission = regionFolder:FindFirstChild("Sniff")
                if sniffMission and sniffMission.Value then
                    missionCompleted = true
                    print("‚úÖ Sniff –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                else
                    print("‚è≥ Sniff... (" .. attempts .. "/40)")
                end
            end
            
            if not missionCompleted then
                wait(1.5)
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ ConcealScent
    function completeConcealScentMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º ConcealScent –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        teleportPlayer(concealScentPosition)
        wait(2)
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 60 do
            attempts = attempts + 1
            
            useFoodAndWater()
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
            local currentTime = tick()
            if currentTime - lastDeathCheck >= 10 then
                if checkDeathDuringFarm() then
                    print("üîÑ –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—é –º–∏—Å—Å–∏–∏ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏...")
                    wait(2)
                end
                lastDeathCheck = currentTime
            end
            
            local nearestMud = findNearestMud()
            if nearestMud then
                local mudArgs = { nearestMud }
                MudRemote:FireServer(unpack(mudArgs))
                print("üõÅ –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω Mud: " .. nearestMud.Name)
            else
                print("‚ö†Ô∏è Mud –Ω–µ –Ω–∞–π–¥–µ–Ω")
            end
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local concealMission = regionFolder:FindFirstChild("ConcealScent")
                if concealMission and concealMission.Value then
                    missionCompleted = true
                    print("‚úÖ ConcealScent –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                else
                    print("‚è≥ ConcealScent... (" .. attempts .. "/60)")
                end
            end
            
            if not missionCompleted then
                wait(1)
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ EatFoodDrinkWater
    function completeEatFoodDrinkWaterMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º EatFoodDrinkWater –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 30 do
            attempts = attempts + 1
            
            useFoodAndWater()
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
            local currentTime = tick()
            if currentTime - lastDeathCheck >= 10 then
                if checkDeathDuringFarm() then
                    print("üîÑ –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—é –º–∏—Å—Å–∏–∏ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏...")
                    wait(2)
                end
                lastDeathCheck = currentTime
            end
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local eatMission = regionFolder:FindFirstChild("EatFoodDrinkWater")
                if eatMission and eatMission.Value then
                    missionCompleted = true
                    print("‚úÖ EatFoodDrinkWater –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                else
                    print("‚è≥ EatFoodDrinkWater... (" .. attempts .. "/30)")
                end
            end
            
            if not missionCompleted then
                wait(2)
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ AttackOrHealCreatureOrNPC
    function completeAttackOrHealMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º AttackOrHealCreatureOrNPC –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        if teleportToRandomNPCSpawn() then
            wait(2)
            changeRegion(regionName)
        end
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 100 do
            attempts = attempts + 1
            
            useFoodAndWater()
            attackNPC()
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
            local currentTime = tick()
            if currentTime - lastDeathCheck >= 10 then
                if checkDeathDuringFarm() then
                    print("üîÑ –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—é –º–∏—Å—Å–∏–∏ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏...")
                    wait(2)
                end
                lastDeathCheck = currentTime
            end
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local attackMission = regionFolder:FindFirstChild("AttackOrHealCreatureOrNPC")
                if attackMission and attackMission.Value then
                    missionCompleted = true
                    print("‚úÖ AttackOrHealCreatureOrNPC –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                    break
                end
            end
            
            if not missionCompleted then
                wait(0.5)
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ DistanceTravelled
    function completeDistanceTravelledMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º DistanceTravelled –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 100 do
            attempts = attempts + 1
            
            useFoodAndWater()
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
            local currentTime = tick()
            if currentTime - lastDeathCheck >= 10 then
                if checkDeathDuringFarm() then
                    print("üîÑ –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—é –º–∏—Å—Å–∏–∏ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏...")
                    wait(2)
                end
                lastDeathCheck = currentTime
            end
            
            for i, pos in ipairs(teleportPositions) do
                teleportPlayer(pos)
                wait(2)
                
                if i == 1 then
                    changeRegion(regionName)
                end
                
                local regionFolder = regionMissions:FindFirstChild(regionName)
                if regionFolder then
                    local distanceMission = regionFolder:FindFirstChild("DistanceTravelled")
                    if distanceMission and distanceMission.Value then
                        missionCompleted = true
                        print("‚úÖ DistanceTravelled –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                        break
                    end
                end
            end
            
            if missionCompleted then
                break
            else
                print("‚è≥ DistanceTravelled... (" .. attempts .. "/100)")
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ TimePlayed
    function completeTimePlayedMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º TimePlayed –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        print("‚è∞ –û–∂–∏–¥–∞–µ–º...")
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local checkCount = 0
        
        while not missionCompleted do
            checkCount = checkCount + 1
            useFoodAndWater()
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
            local currentTime = tick()
            if currentTime - lastDeathCheck >= 10 then
                if checkDeathDuringFarm() then
                    print("üîÑ –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—é –º–∏—Å—Å–∏–∏ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏...")
                    wait(2)
                end
                lastDeathCheck = currentTime
            end
            
            local otherMissions = getUncompletedMissions()
            for _, mission in ipairs(otherMissions) do
                if mission.missionName ~= "TimePlayed" then
                    print("‚ö†Ô∏è –ü–æ—è–≤–∏–ª–∏—Å—å –¥—Ä—É–≥–∏–µ –º–∏—Å—Å–∏–∏! –ü—Ä–µ—Ä—ã–≤–∞–µ–º TimePlayed")
                    return false
                end
            end
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local timeMission = regionFolder:FindFirstChild("TimePlayed")
                if timeMission and timeMission.Value then
                    missionCompleted = true
                    print("‚úÖ TimePlayed –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                    break
                else
                    print("‚è≥ TimePlayed... (" .. checkCount .. " –ø—Ä–æ–≤–µ—Ä–æ–∫)")
                end
            end
            
            wait(10)
        end
        
        return missionCompleted
    end
    
    -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –º–∏—Å—Å–∏–π
    function autoCompleteAllMissions()
        print("üéØ –ó–∞–ø—É—Å–∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è –Ω–µ–≤—ã–ø–æ–ª–Ω–µ–Ω–Ω—ã—Ö –º–∏—Å—Å–∏–π...")
        
        while true do
            local uncompleted = getUncompletedMissions()
            
            if #uncompleted == 0 then
                print("üéâ –í—Å–µ –º–∏—Å—Å–∏–∏ –≤—ã–ø–æ–ª–Ω–µ–Ω—ã!")
                print("üîÑ –ñ–¥–µ–º –ø–æ—è–≤–ª–µ–Ω–∏—è –Ω–æ–≤—ã—Ö –º–∏—Å—Å–∏–π...")
                wait(10)
            else
                print("üìã –ù–∞–π–¥–µ–Ω–æ –Ω–µ–≤—ã–ø–æ–ª–Ω–µ–Ω–Ω—ã—Ö –º–∏—Å—Å–∏–π: " .. #uncompleted)
                for i, mission in ipairs(uncompleted) do
                    print(i .. ". " .. mission.regionName .. " - " .. mission.missionName)
                end
                
                for i, mission in ipairs(uncompleted) do
                    print("\n" .. string.rep("-", 40))
                    print("üéØ –ú–∏—Å—Å–∏—è " .. i .. "/" .. #uncompleted .. ": " .. mission.regionName .. " - " .. mission.missionName)
                    print(string.rep("-", 40))
                    
                    local success = false
                    
                    if mission.missionName == "Sniff" then
                        success = completeSniffMission(mission.regionName)
                    elseif mission.missionName == "ConcealScent" then
                        success = completeConcealScentMission(mission.regionName)
                    elseif mission.missionName == "EatFoodDrinkWater" then
                        success = completeEatFoodDrinkWaterMission(mission.regionName)
                    elseif mission.missionName == "AttackOrHealCreatureOrNPC" then
                        success = completeAttackOrHealMission(mission.regionName)
                    elseif mission.missionName == "DistanceTravelled" then
                        success = completeDistanceTravelledMission(mission.regionName)
                    elseif mission.missionName == "TimePlayed" then
                        success = completeTimePlayedMission(mission.regionName)
                    else
                        print("‚ùì –ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π —Ç–∏–ø –º–∏—Å—Å–∏–∏: " .. mission.missionName)
                    end
                    
                    if success then
                        print("‚úÖ –£—Å–ø–µ—à–Ω–æ –≤—ã–ø–æ–ª–Ω–µ–Ω–æ!")
                    else
                        print("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –≤—ã–ø–æ–ª–Ω–∏—Ç—å")
                    end
                    
                    wait(1)
                end
                
                print("\nüîÑ –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ –Ω–æ–≤—ã—Ö –º–∏—Å—Å–∏–π...")
                wait(5)
            end
        end
    end
    
    -- –ó–∞–ø—É—Å–∫–∞–µ–º –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–π
    autoCompleteAllMissions()
end

-- ========== –ì–õ–ê–í–ù–ê–Ø –õ–û–ì–ò–ö–ê ==========
print("üîß –®–∞–≥ 0: –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å...")
local deathHandled = handleDeath()

if deathHandled then
    print("‚è≥ –û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–º–µ—Ä—Ç–∏ –∑–∞–≤–µ—Ä—à–µ–Ω–∞, –∂–¥–µ–º 2 —Å–µ–∫—É–Ω–¥—ã...")
    wait(2)
end

print("üîß –®–∞–≥ 1: –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª...")
local tutorialCompleted = completeTutorial()

if tutorialCompleted then
    print("‚è≥ –¢—É—Ç–æ—Ä–∏–∞–ª –∑–∞–≤–µ—Ä—à–µ–Ω, –∂–¥–µ–º 2 —Å–µ–∫—É–Ω–¥—ã...")
    wait(2)
end

print("üîß –®–∞–≥ 2: –ó–∞–∫—Ä—ã–≤–∞–µ–º –º–µ—à–∞—é—â–∏–µ GUI...")
local closedSomething = closeAnnoyingGUIs()

if closedSomething then
    print("‚è≥ –®–∞–≥ 3: –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã...")
    
    -- –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã –∏ –ø—Ä–æ–≤–µ—Ä—è–µ–º –Ω–µ –ø–æ—è–≤–∏–ª–∏—Å—å –ª–∏ –Ω–æ–≤—ã–µ –º–µ—à–∞—é—â–∏–µ GUI
    local startTime = tick()
    local waitTime = 3
    
    while tick() - startTime < waitTime do
        if hasActiveAnnoyingGUIs() then
            print("‚ö†Ô∏è –û–±–Ω–∞—Ä—É–∂–µ–Ω—ã –Ω–æ–≤—ã–µ –º–µ—à–∞—é—â–∏–µ GUI, –∑–∞–∫—Ä—ã–≤–∞–µ–º...")
            closeAnnoyingGUIs()
            startTime = tick() -- –°–±—Ä–∞—Å—ã–≤–∞–µ–º —Ç–∞–π–º–µ—Ä
        end
        wait(0.5)
    end
    
    print("‚úÖ –®–∞–≥ 4: –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ –º–µ—à–∞—é—â–∏—Ö GUI –Ω–µ—Ç...")
    
    if not hasActiveAnnoyingGUIs() then
        print("üéâ –ù–µ—Ç –º–µ—à–∞—é—â–∏—Ö GUI, –≤—ã–ø–æ–ª–Ω—è–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏!")
        doOurClicks()
        
        -- –ü–æ—Å–ª–µ –Ω–∞—à–∏—Ö –∫–ª–∏–∫–æ–≤ –∑–∞–ø—É—Å–∫–∞–µ–º —Ñ–∞—Ä–º –º–∏—Å—Å–∏–π
        print("\n" .. string.rep("=", 50))
        print("‚è≥ –ü–µ—Ä–µ—Ö–æ–¥ –∫ —Ñ–∞—Ä–º—É –º–∏—Å—Å–∏–π —á–µ—Ä–µ–∑ 2 —Å–µ–∫—É–Ω–¥—ã...")
        print(string.rep("=", 50))
        wait(2)
        
        -- –ó–ê–ü–£–°–ö–ê–ï–ú –§–ê–†–ú –ú–ò–°–°–ò–ô
        startMissionFarm()
        
    else
        print("‚ùå –í—Å–µ –µ—â–µ –µ—Å—Ç—å –º–µ—à–∞—é—â–∏–µ GUI, –ø—Ä–æ–ø—É—Å–∫–∞–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏")
        -- –í—Å–µ —Ä–∞–≤–Ω–æ –∑–∞–ø—É—Å–∫–∞–µ–º —Ñ–∞—Ä–º
        wait(2)
        startMissionFarm()
    end
else
    print("‚úÖ –ù–µ—Ç –º–µ—à–∞—é—â–∏—Ö GUI, —Å—Ä–∞–∑—É –≤—ã–ø–æ–ª–Ω—è–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏!")
    doOurClicks()
    
    -- –ü–æ—Å–ª–µ –Ω–∞—à–∏—Ö –∫–ª–∏–∫–æ–≤ –∑–∞–ø—É—Å–∫–∞–µ–º —Ñ–∞—Ä–º –º–∏—Å—Å–∏–π
    print("\n" .. string.rep("=", 50))
    print("‚è≥ –ü–µ—Ä–µ—Ö–æ–¥ –∫ —Ñ–∞—Ä–º—É –º–∏—Å—Å–∏–π —á–µ—Ä–µ–∑ 2 —Å–µ–∫—É–Ω–¥—ã...")
    print(string.rep("=", 50))
    wait(2)
    
    -- –ó–ê–ü–£–°–ö–ê–ï–ú –§–ê–†–ú –ú–ò–°–°–ò–ô
    startMissionFarm()
end

print("\n" .. string.rep("=", 50))
print("‚ú® –í–ï–°–¨ –°–ö–†–ò–ü–¢ –ó–ê–í–ï–†–®–ï–ù!")
print(string.rep("=", 50))
