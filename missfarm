-- –ü–æ–ª–Ω—ã–π —Å–∫—Ä–∏–ø—Ç: —Ç—É—Ç–æ—Ä–∏–∞–ª, —Å–º–µ—Ä—Ç—å, –∫–ª–∏–∫–∏, —Ñ–∞—Ä–º –º–∏—Å—Å–∏–π
local VirtualInputManager = game:GetService("VirtualInputManager")
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

wait(7) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π —Å—Ç–∞—Ä—Ç–æ–≤—ã–π –ö–î
print("üéÆ –ó–∞–ø—É—Å–∫ —Å–∫—Ä–∏–ø—Ç–∞")

-- ========== –§–£–ù–ö–¶–ò–ò –î–õ–Ø –ö–õ–ò–ö–ê ==========
-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∫–ª–∏–∫–∞ –ø–æ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º
local function clickAtPosition(posX, posY)
    VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, true, game, 1)
    wait(0.1) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    VirtualInputManager:SendMouseButtonEvent(posX, posY, 0, false, game, 1)
end

-- –§—É–Ω–∫—Ü–∏—è –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä–µ —ç–∫—Ä–∞–Ω–∞
local function click()
    local screen = workspace.CurrentCamera.ViewportSize
    clickAtPosition(screen.X/2, screen.Y/2)
end

-- ========== –£–õ–£–ß–®–ï–ù–ù–ê–Ø –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ö–õ–ò–ö–û–í –ü–û –ö–ù–û–ü–ö–ê–ú ==========
local function clickButtonEnhanced(button, buttonName)
    if button then
        print("‚úÖ –ù–∞–π–¥–µ–Ω–∞ –∫–Ω–æ–ø–∫–∞: " .. buttonName)
        
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ —Å–≤–æ–π—Å—Ç–≤–∞
        local originalSize = button.Size
        local originalPosition = button.Position
        local originalAnchorPoint = button.AnchorPoint
        local originalVisible = button.Visible
        local originalZIndex = button.ZIndex
        
        -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–Ω–æ–ø–∫—É –û–ß–ï–ù–¨ —Å–∏–ª—å–Ω–æ –∏ –¥–µ–ª–∞–µ–º –≤–∏–¥–∏–º–æ–π
        button.Size = UDim2.new(100, 0, 100, 0) -- –û–ß–ï–ù–¨ –±–æ–ª—å—à–æ–π —Ä–∞–∑–º–µ—Ä
        button.Position = UDim2.new(0.5, 0, 0.5, 0) -- –¶–µ–Ω—Ç—Ä–∏—Ä—É–µ–º
        button.AnchorPoint = Vector2.new(0.5, 0.5)
        button.Visible = true
        button.ZIndex = 99999 -- –°–∞–º—ã–π –≤—ã—Å–æ–∫–∏–π ZIndex
        
        wait(1) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î –¥–ª—è –≥–∞—Ä–∞–Ω—Ç–∏–∏
        
        -- –ö–ª–∏–∫–∞–µ–º –≤ —Ü–µ–Ω—Ç—Ä —É–≤–µ–ª–∏—á–µ–Ω–Ω–æ–π –∫–Ω–æ–ø–∫–∏
        local screen = workspace.CurrentCamera.ViewportSize
        local centerX = screen.X / 2
        local centerY = screen.Y / 2
        
        print("üìç –ö–ª–∏–∫–∞–µ–º –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞ –¥–ª—è –∫–Ω–æ–ø–∫–∏ " .. buttonName .. ": " .. math.floor(centerX) .. ", " .. math.floor(centerY))
        clickAtPosition(centerX, centerY)
        
        -- –ñ–¥–µ–º –ø–µ—Ä–µ–¥ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ–º
        wait(0.5)
        
        -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Å–≤–æ–π—Å—Ç–≤–∞
        button.Size = originalSize
        button.Position = originalPosition
        button.AnchorPoint = originalAnchorPoint
        button.Visible = originalVisible
        button.ZIndex = originalZIndex
        
        print("‚úÖ –ö–ª–∏–∫ –ø–æ –∫–Ω–æ–ø–∫–µ " .. buttonName .. " –≤—ã–ø–æ–ª–Ω–µ–Ω")
        return true
    else
        print("‚ö†Ô∏è –ö–Ω–æ–ø–∫–∞ " .. buttonName .. " –Ω–µ –Ω–∞–π–¥–µ–Ω–∞")
        return false
    end
end

-- ========== –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –û–ë–†–ê–ë–û–¢–ö–ò GUI ==========
local function handleSpecialGUIs()
    print("\nüéÅ –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å–ø–µ—Ü–∏–∞–ª—å–Ω—ã—Ö GUI...")
    
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    local handled = false
    
    -- –°–ø–µ—Ü–∏–∞–ª—å–Ω—ã–µ GUI –¥–ª—è –æ–±—Ä–∞–±–æ—Ç–∫–∏
    local specialGuis = {
        {
            name = "AdventCalendarGui",
            path = "ContainerFrame.CloseButton.UpperLabel",
            clicks = 1,
            delay = 2
        },
        {
            name = "DailyLoginGui", 
            path = "ContainerFrame.CurrencyFrame.CloseButton.UpperLabel",
            clicks = 2,  -- 2 –∫–ª–∏–∫–∞ —Å –∑–∞–¥–µ—Ä–∂–∫–æ–π
            delay = 2
        },
        {
            name = "UpdateLogGui",
            path = "ContainerFrame.CloseButton.UpperLabel",
            clicks = 1,
            delay = 2
        },
        {
            name = "PromptGui",
            special = true,  -- –û—Å–æ–±—ã–π —Å–ª—É—á–∞–π, –ø—Ä–æ—Å—Ç–æ —Å–∫—Ä—ã–≤–∞–µ–º
            delay = 1
        }
    }
    
    for _, guiInfo in ipairs(specialGuis) do
        local gui = playerGui:FindFirstChild(guiInfo.name)
        if gui and gui.Enabled == true then
            print("üéØ " .. guiInfo.name .. " –∞–∫—Ç–∏–≤–µ–Ω! –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º...")
            handled = true
            
            if guiInfo.special then
                -- –î–ª—è PromptGui –ø—Ä–æ—Å—Ç–æ —Å–∫—Ä—ã–≤–∞–µ–º
                gui.Enabled = false
                print("‚úÖ " .. guiInfo.name .. " —Å–∫—Ä—ã—Ç")
                wait(guiInfo.delay)
            else
                -- –î–ª—è –¥—Ä—É–≥–∏—Ö GUI –∏—â–µ–º –∫–Ω–æ–ø–∫—É –∑–∞–∫—Ä—ã—Ç–∏—è –∏ –∫–ª–∏–∫–∞–µ–º
                local button = gui
                local pathParts = string.split(guiInfo.path, ".")
                
                for _, part in ipairs(pathParts) do
                    if button then
                        button = button:FindFirstChild(part)
                    end
                end
                
                if button then
                    -- –í—ã–ø–æ–ª–Ω—è–µ–º –Ω—É–∂–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤
                    for i = 1, guiInfo.clicks do
                        print("üìç –ö–ª–∏–∫ " .. i .. "/" .. guiInfo.clicks .. " –ø–æ " .. guiInfo.name)
                        local clicked = clickButtonEnhanced(button, guiInfo.name)
                        
                        if not clicked then
                            print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å, –¥–µ–ª–∞–µ–º –æ–±—ã—á–Ω—ã–π –∫–ª–∏–∫")
                            click()
                        end
                        
                        if i < guiInfo.clicks then
                            wait(guiInfo.delay)
                        end
                    end
                    print("‚úÖ " .. guiInfo.name .. " –∑–∞–∫—Ä—ã—Ç")
                else
                    print("‚ö†Ô∏è –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è –Ω–µ –Ω–∞–π–¥–µ–Ω–∞ –≤ " .. guiInfo.name .. ", –¥–µ–ª–∞–µ–º –æ–±—ã—á–Ω—ã–π –∫–ª–∏–∫")
                    click()
                end
                
                wait(guiInfo.delay)
            end
        end
    end
    
    if handled then
        print("‚è≥ –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã –ø–æ—Å–ª–µ –∑–∞–∫—Ä—ã—Ç–∏—è GUI...")
        wait(3)
    else
        print("‚úÖ –ù–µ—Ç –∞–∫—Ç–∏–≤–Ω—ã—Ö —Å–ø–µ—Ü–∏–∞–ª—å–Ω—ã—Ö GUI")
    end
    
    return handled
end

-- ========== –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ö–õ–ò–ö–û–í –ü–û–°–õ–ï –°–û–ó–î–ê–ù–ò–Ø –°–õ–û–¢–ê ==========
local function clickAfterSlotCreation()
    print("\nüéØ –í—ã–ø–æ–ª–Ω—è–µ–º –∫–ª–∏–∫–∏ –ø–æ—Å–ª–µ —Å–æ–∑–¥–∞–Ω–∏—è —Å–ª–æ—Ç–∞...")
    
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    print("‚è≥ –ñ–¥–µ–º 5 —Å–µ–∫—É–Ω–¥ –ø–µ—Ä–µ–¥ –∫–ª–∏–∫–∞–º–∏...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    wait(5)
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–ø–µ—Ü–∏–∞–ª—å–Ω—ã–µ GUI
    handleSpecialGUIs()
    
    -- –ü—Ä–æ–±—É–µ–º –æ—Ç–∫—Ä—ã—Ç—å SaveSelectionGui –µ—Å–ª–∏ –æ–Ω –Ω–µ –æ—Ç–∫—Ä—ã—Ç
    local saveSelectionGui = playerGui:FindFirstChild("SaveSelectionGui")
    
    if not saveSelectionGui or not saveSelectionGui.Enabled then
        print("üìÇ SaveSelectionGui –Ω–µ –æ—Ç–∫—Ä—ã—Ç, –æ—Ç–∫—Ä—ã–≤–∞–µ–º...")
        click() -- –ö–ª–∏–∫ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞
        wait(3) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    end
    
    -- –ñ–¥–µ–º –µ—â–µ –Ω–µ–º–Ω–æ–≥–æ
    wait(3)
    
    -- 1. –ò—â–µ–º –∏ –∫–ª–∏–∫–∞–µ–º –ø–æ –ø–µ—Ä–≤–æ–º—É —ç–ª–µ–º–µ–Ω—Ç—É (PagesFrame["1"])
    print("\n1. –ò—â–µ–º —ç–ª–µ–º–µ–Ω—Ç PagesFrame['1']...")
    
    local elem1 = nil
    saveSelectionGui = playerGui:FindFirstChild("SaveSelectionGui")
    
    if saveSelectionGui and saveSelectionGui.Enabled then
        -- –ò—â–µ–º —á–µ—Ä–µ–∑ –±–µ–∑–æ–ø–∞—Å–Ω—ã–π –ø–æ–∏—Å–∫
        local success, result = pcall(function()
            return saveSelectionGui.ContainerFrame.AllSlotsFrame.PagesFrame["1"]
        end)
        
        if success and result then
            elem1 = result
        end
    end
    
    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–ª—É—á—à–µ–Ω–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –∫–ª–∏–∫–∞
    local clicked1 = clickButtonEnhanced(elem1, "PagesFrame['1']")
    
    if not clicked1 then
        print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ PagesFrame['1'], –∫–ª–∏–∫–∞–µ–º –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
        click()
    end
    
    wait(3) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    
    -- 2. –ò—â–µ–º –∏ –∫–ª–∏–∫–∞–µ–º –ø–æ PlayButton.UpperLabel
    print("\n2. –ò—â–µ–º –∫–Ω–æ–ø–∫—É PlayButton.UpperLabel...")
    
    local playButtonUpperLabel = nil
    if saveSelectionGui and saveSelectionGui.Enabled then
        -- –ò—â–µ–º —á–µ—Ä–µ–∑ –±–µ–∑–æ–ø–∞—Å–Ω—ã–π –ø–æ–∏—Å–∫
        local success, result = pcall(function()
            return saveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame["1"].InnerFrame.CreatureFrame.ButtonsFrame.PlayButton.UpperLabel
        end)
        
        if success and result then
            playButtonUpperLabel = result
        else
            -- –ü—Ä–æ–±—É–µ–º –∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –ø—É—Ç—å
            local success2, result2 = pcall(function()
                return saveSelectionGui:FindFirstChild("ContainerFrame", true):FindFirstChild("AllSlotsFrame", true):FindFirstChild("SlotsFrame", true):FindFirstChild("1", true):FindFirstChild("InnerFrame", true):FindFirstChild("CreatureFrame", true):FindFirstChild("ButtonsFrame", true):FindFirstChild("PlayButton", true):FindFirstChild("UpperLabel", true)
            end)
            
            if success2 and result2 then
                playButtonUpperLabel = result2
            end
        end
    end
    
    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–ª—É—á—à–µ–Ω–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –∫–ª–∏–∫–∞
    local clickedPlayButton = clickButtonEnhanced(playButtonUpperLabel, "PlayButton.UpperLabel")
    
    if clickedPlayButton then
        -- –í—Ç–æ—Ä–æ–π –∫–ª–∏–∫ —Å –Ω–µ–±–æ–ª—å—à–æ–π –∑–∞–¥–µ—Ä–∂–∫–æ–π
        wait(0.5)
        click()
    else
        print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ PlayButton.UpperLabel, –∏—â–µ–º —Å–∞–º—É –∫–Ω–æ–ø–∫—É PlayButton...")
        
        -- –ü—Ä–æ–±—É–µ–º –Ω–∞–π—Ç–∏ —Å–∞–º—É –∫–Ω–æ–ø–∫—É PlayButton
        local playButton = nil
        if saveSelectionGui and saveSelectionGui.Enabled then
            local success, result = pcall(function()
                return saveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame["1"].InnerFrame.CreatureFrame.ButtonsFrame.PlayButton
            end)
            
            if success and result then
                playButton = result
                local clickedPlay = clickButtonEnhanced(playButton, "PlayButton")
                
                if clickedPlay then
                    wait(0.5)
                    click()
                else
                    print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ PlayButton, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
                    click()
                    wait(0.5)
                    click()
                end
            else
                print("‚ö†Ô∏è PlayButton –Ω–µ –Ω–∞–π–¥–µ–Ω, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
                click()
                wait(0.5)
                click()
            end
        else
            print("‚ö†Ô∏è SaveSelectionGui –Ω–µ –∞–∫—Ç–∏–≤–µ–Ω, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
            click()
            wait(0.5)
            click()
        end
    end
    
    wait(3)
    print("üéØ –ö–ª–∏–∫–∏ –ø–æ—Å–ª–µ —Å–æ–∑–¥–∞–Ω–∏—è —Å–ª–æ—Ç–∞ –∑–∞–≤–µ—Ä—à–µ–Ω—ã!")
    return true
end

-- ========== –û–ë–†–ê–ë–û–¢–ö–ê –¢–£–¢–û–†–ò–ê–õ–ê (–° –ü–†–û–í–ï–†–ö–û–ô TutorialCompletedAt) ==========
local function handleTutorial()
    print("\nüìö –ü—Ä–æ–≤–µ—Ä–∫–∞ —Ç—É—Ç–æ—Ä–∏–∞–ª–∞...")
    
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º TutorialCompletedAt.Value
    local tutorialCompletedAt = 1  -- –ü–æ —É–º–æ–ª—á–∞–Ω–∏—é —Å—á–∏—Ç–∞–µ–º —á—Ç–æ —Ç—É—Ç–æ—Ä–∏–∞–ª –∑–∞–≤–µ—Ä—à–µ–Ω
    
    if playerGui:FindFirstChild("Data") then
        local data = playerGui.Data
        if data:FindFirstChild("Tutorial") then
            local tutorial = data.Tutorial
            if tutorial:FindFirstChild("TutorialCompletedAt") then
                tutorialCompletedAt = tutorial.TutorialCompletedAt.Value
                print("üìä TutorialCompletedAt.Value = " .. tostring(tutorialCompletedAt))
            end
        end
    end
    
    -- –ï—Å–ª–∏ TutorialCompletedAt.Value = 0, –∑–Ω–∞—á–∏—Ç —Ç—É—Ç–æ—Ä–∏–∞–ª –Ω–µ –∑–∞–≤–µ—Ä—à–µ–Ω
    if tutorialCompletedAt == 0 then
        print("üìö –¢—É—Ç–æ—Ä–∏–∞–ª –ù–ï –∑–∞–≤–µ—Ä—à–µ–Ω (Value = 0)! –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º...")
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º TutorialGui
        local tutorialGui = playerGui:FindFirstChild("TutorialGui")
        if tutorialGui and tutorialGui.Enabled == true then
            print("üìö TutorialGui –∞–∫—Ç–∏–≤–µ–Ω! –ù–∞—á–∏–Ω–∞–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª...")
            
            -- 1. –ö–õ–ò–ö –ü–û –ö–ù–û–ü–ö–ï SELECT –í TUTORIALGUI
            print("1. –ò—â–µ–º –∫–Ω–æ–ø–∫—É SelectButton.UpperLabel...")
            
            local upperLabel1 = playerGui.TutorialGui.SelectionFrame.SlotsFrame.Sochuri.InnerFrame.SelectButton.UpperLabel
            
            -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–ª—É—á—à–µ–Ω–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –∫–ª–∏–∫–∞
            local clickedSelect = clickButtonEnhanced(upperLabel1, "SelectButton.UpperLabel")
            
            if not clickedSelect then
                print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ SelectButton, –∫–ª–∏–∫–∞–µ–º –≤ –ø—Ä–∞–≤—ã–π –Ω–∏–∂–Ω–∏–π —É–≥–æ–ª")
                local screen = workspace.CurrentCamera.ViewportSize
                local cornerX = screen.X - 50
                local cornerY = screen.Y - 50
                clickAtPosition(cornerX, cornerY)
            end
            
            wait(15) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
            
            -- 2. –ö–õ–ò–ö –ü–û –ö–ù–û–ü–ö–ï PLAY –í EDITORGUI
            print("\n2. –ò—â–µ–º –∫–Ω–æ–ø–∫—É PlayButton.UpperLabel...")
            
            local editorGui = playerGui:FindFirstChild("EditorGui")
            if editorGui then
                if not editorGui.Enabled then
                    editorGui.Enabled = true
                    wait(2) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
                end
                
                local upperLabel2 = editorGui.EditFrame.BottomFrame.ButtonsFrame.PlayButton.UpperLabel
                
                -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–ª—É—á—à–µ–Ω–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –∫–ª–∏–∫–∞
                local clickedPlay = clickButtonEnhanced(upperLabel2, "PlayButton.UpperLabel")
                
                if clickedPlay then
                    -- –í—Ç–æ—Ä–æ–π –∫–ª–∏–∫
                    wait(0.5)
                    click()
                else
                    print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ PlayButton, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
                    click()
                    wait(0.5)
                    click()
                end
            else
                print("‚ö†Ô∏è EditorGui –Ω–µ –Ω–∞–π–¥–µ–Ω, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
                click()
                wait(0.5)
                click()
            end
            
            -- –ñ–¥–µ–º 2 —Å–µ–∫—É–Ω–¥—ã –ø–æ—Å–ª–µ —Å–ø–∞–≤–Ω–∞ –ø–µ—Ä–µ–¥ –æ—Ç–ø—Ä–∞–≤–∫–æ–π Remote –∑–∞–ø—Ä–æ—Å–æ–≤
            print("‚è≥ –ñ–¥–µ–º 2 —Å–µ–∫—É–Ω–¥—ã –ø–æ—Å–ª–µ —Å–ø–∞–≤–Ω–∞...")
            wait(2)
            
            -- 3. REMOTE –ó–ê–ü–†–û–°–´ –î–õ–Ø –ó–ê–í–ï–†–®–ï–ù–ò–Ø –¢–£–¢–û–†–ò–ê–õ–ê
            print("\n3. –í—ã–ø–æ–ª–Ω—è–µ–º Remote –∑–∞–ø—Ä–æ—Å—ã –¥–ª—è –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è —Ç—É—Ç–æ—Ä–∏–∞–ª–∞...")
            
            -- SniffWater
            local success1, error1 = pcall(function()
                local args = {"SniffWater"}
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("PostTutorialStageRemote"):FireServer(unpack(args))
                print("‚úÖ SniffWater –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
            end)
            
            if not success1 then
                print("‚ö†Ô∏è –û—à–∏–±–∫–∞ SniffWater: " .. tostring(error1))
            end
            
            wait(0.5)
            
            -- SniffFood
            local success2, error2 = pcall(function()
                local args = {"SniffFood"}
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("PostTutorialStageRemote"):FireServer(unpack(args))
                print("‚úÖ SniffFood –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
            end)
            
            if not success2 then
                print("‚ö†Ô∏è –û—à–∏–±–∫–∞ SniffFood: " .. tostring(error2))
            end
            
            wait(0.5)
            
            -- SurvivalBasics
            local successSurvival, errorSurvival = pcall(function()
                local ohString1 = "SurvivalBasics"
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SetTutorialStageRemote"):InvokeServer(ohString1)
                print("‚úÖ SurvivalBasics –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
            end)
            
            if not successSurvival then
                print("‚ö†Ô∏è –û—à–∏–±–∫–∞ SurvivalBasics: " .. tostring(errorSurvival))
            end
            
            wait(0.1)
            
            -- Complete
            local successComplete, errorComplete = pcall(function()
                local ohString1 = "Complete"
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SetTutorialStageRemote"):InvokeServer(ohString1)
                print("‚úÖ Complete –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
            end)
            
            if not successComplete then
                print("‚ö†Ô∏è –û—à–∏–±–∫–∞ Complete: " .. tostring(errorComplete))
            end
            
            -- –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–û: –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∏–∑–º–µ–Ω–∏–ª—Å—è –ª–∏ TutorialCompletedAt
            wait(2)
            
            local newTutorialCompletedAt = tutorialCompletedAt
            if playerGui:FindFirstChild("Data") then
                local data = playerGui.Data
                if data:FindFirstChild("Tutorial") then
                    local tutorial = data.Tutorial
                    if tutorial:FindFirstChild("TutorialCompletedAt") then
                        newTutorialCompletedAt = tutorial.TutorialCompletedAt.Value
                        print("üìä –ù–æ–≤–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ TutorialCompletedAt.Value = " .. tostring(newTutorialCompletedAt))
                    end
                end
            end
            
            if newTutorialCompletedAt > 0 then
                print("üéâ –¢–£–¢–û–†–ò–ê–õ –£–°–ü–ï–®–ù–û –ó–ê–í–ï–†–®–ï–ù!")
            else
                print("‚ö†Ô∏è –¢—É—Ç–æ—Ä–∏–∞–ª –≤—ã–ø–æ–ª–Ω–µ–Ω, –Ω–æ TutorialCompletedAt –≤—Å–µ –µ—â–µ 0")
            end
            
            return true
            
        else
            print("‚ö†Ô∏è TutorialGui –Ω–µ –∞–∫—Ç–∏–≤–µ–Ω, –Ω–æ TutorialCompletedAt = 0")
            return false
        end
    else
        print("‚úÖ –¢—É—Ç–æ—Ä–∏–∞–ª —É–∂–µ –∑–∞–≤–µ—Ä—à–µ–Ω (TutorialCompletedAt.Value = " .. tostring(tutorialCompletedAt) .. ")")
        return false
    end
end

-- ========== –û–ë–†–ê–ë–û–¢–ö–ê –°–ú–ï–†–¢–ò ==========
local function handleDeath()
    print("\nüíÄ –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å–º–µ—Ä—Ç–∏...")
    
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    local deathGui = playerGui:FindFirstChild("DeathGui")
    if deathGui and deathGui.Enabled == true then
        print("üíÄ DeathGui –∞–∫—Ç–∏–≤–µ–Ω! –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º...")
        wait(10)
        local returnButton = deathGui.ContainerFrame.BottomFrame.ButtonsFrame.Return.UpperLabel
        
        -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–ª—É—á—à–µ–Ω–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –∫–ª–∏–∫–∞ –ø–æ Return
        local clickedReturn = clickButtonEnhanced(returnButton, "Return.UpperLabel")
        
        if clickedReturn then
            -- –í—Ç–æ—Ä–æ–π –∫–ª–∏–∫ –Ω–∞ Return
            wait(0.5)
            click()
        else
            print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ Return, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
            click()
            wait(0.5)
            click()
        end
        
        -- –ñ–¥–µ–º –ø–æ–∫–∞ GUI –ø–æ–ª–Ω–æ—Å—Ç—å—é –∑–∞–∫—Ä–æ–µ—Ç—Å—è
        print("‚è≥ –ñ–¥–µ–º 7 —Å–µ–∫—É–Ω–¥...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        wait(7)
        
        -- 1. –í–´–ó–´–í–ê–ï–ú DESPAWNREMOTE –î–õ–Ø –í–´–•–û–î–ê
        print("üö™ –í—ã–∑—ã–≤–∞–µ–º DespawnRemote –¥–ª—è –≤—ã—Ö–æ–¥–∞...")
        local despawnSuccess, despawnError = pcall(function()
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DespawnRemote"):InvokeServer()
            return true
        end)
        
        if despawnSuccess then
            print("‚úÖ DespawnRemote —É—Å–ø–µ—à–Ω–æ –≤—ã–∑–≤–∞–Ω")
        else
            print("‚ùå –û—à–∏–±–∫–∞ DespawnRemote: " .. tostring(despawnError))
        end
        
        wait(3) -- –ñ–¥–µ–º –ø–æ—Å–ª–µ –¥–µ—Å–ø–∞–≤–Ω–∞
        
        -- 2. –£–î–ê–õ–ï–ù–ò–ï –°–õ–û–¢–ê
        print("üóëÔ∏è –£–¥–∞–ª—è–µ–º —Å–ª–æ—Ç Slot1...")
        local deleteSuccess, deleteError = pcall(function()
            local args = {"Slot1", false}
            ReplicatedStorage.Remotes.DeleteSlotRemote:InvokeServer(unpack(args))
            return true
        end)
        
        if deleteSuccess then
            print("‚úÖ –ó–∞–ø—Ä–æ—Å —É–¥–∞–ª–µ–Ω–∏—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        else
            print("‚ùå –û—à–∏–±–∫–∞ —É–¥–∞–ª–µ–Ω–∏—è: " .. tostring(deleteError))
        end
        
        print("‚è≥ –ñ–¥–µ–º 5 —Å–µ–∫—É–Ω–¥ –ø–æ—Å–ª–µ —É–¥–∞–ª–µ–Ω–∏—è...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        wait(5)
        
        -- 3. –°–û–ó–î–ê–ù–ò–ï –ù–û–í–û–ì–û –°–õ–û–¢–ê
        print("‚ûï –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—ã–π —Å–ª–æ—Ç Sochuri...")
        local createSuccess, createError = pcall(function()
            local args = {"Sochuri"}
            ReplicatedStorage.Remotes.CreateSlotRemote:InvokeServer(unpack(args))
            return true
        end)
        
        if createSuccess then
            print("‚úÖ –ó–∞–ø—Ä–æ—Å —Å–æ–∑–¥–∞–Ω–∏—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        else
            print("‚ùå –û—à–∏–±–∫–∞ —Å–æ–∑–¥–∞–Ω–∏—è: " .. tostring(createError))
        end
        
        print("‚è≥ –ñ–¥–µ–º 5 —Å–µ–∫—É–Ω–¥ –ø–æ—Å–ª–µ —Å–æ–∑–¥–∞–Ω–∏—è...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        wait(5)
        
        -- 4. –í–´–ü–û–õ–ù–Ø–ï–ú –ö–õ–ò–ö–ò
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º –∫–ª–∏–∫–∏ –ø–æ—Å–ª–µ —Å–æ–∑–¥–∞–Ω–∏—è —Å–ª–æ—Ç–∞...")
        local clickSuccess = clickAfterSlotCreation()
        
        if clickSuccess then
            print("üéØ –ö–ª–∏–∫–∏ –≤—ã–ø–æ–ª–Ω–µ–Ω—ã!")
            print("‚úÖ –û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–º–µ—Ä—Ç–∏ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!")
            return true
        else
            print("‚ö†Ô∏è –ë—ã–ª–∏ –ø—Ä–æ–±–ª–µ–º—ã —Å –∫–ª–∏–∫–∞–º–∏")
            return true
        end
    else
        print("‚úÖ DeathGui –Ω–µ –∞–∫—Ç–∏–≤–µ–Ω")
        return false
    end
    
    return false
end

-- ========== –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ü–†–û–í–ï–†–ö–ò –°–ú–ï–†–¢–ò –í–û –í–†–ï–ú–Ø –§–ê–†–ú–ê ==========
local function checkDeathDuringFarm()
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    local deathGui = playerGui:FindFirstChild("DeathGui")
    if deathGui and deathGui.Enabled == true then
        print("\nüíÄ –û–ë–ù–ê–†–£–ñ–ï–ù–ê –°–ú–ï–†–¢–¨ –í–û –í–†–ï–ú–Ø –§–ê–†–ú–ê!")
        
        wait(12) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        
        local returnButton = deathGui.ContainerFrame.BottomFrame.ButtonsFrame.Return.UpperLabel
        
        -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–ª—É—á—à–µ–Ω–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –∫–ª–∏–∫–∞ –ø–æ Return
        local clickedReturn = clickButtonEnhanced(returnButton, "Return.UpperLabel")
        
        if clickedReturn then
            -- –í—Ç–æ—Ä–æ–π –∫–ª–∏–∫ –Ω–∞ Return
            wait(0.5)
            click()
        else
            print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ Return, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
            click()
            wait(0.5)
            click()
        end
        
        -- –ñ–¥–µ–º –ø–æ–∫–∞ GUI –ø–æ–ª–Ω–æ—Å—Ç—å—é –∑–∞–∫—Ä–æ–µ—Ç—Å—è
        wait(5) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        
        -- 1. –í–´–ó–´–í–ê–ï–ú DESPAWNREMOTE –î–õ–Ø –í–´–•–û–î–ê
        print("üö™ –í—ã–∑—ã–≤–∞–µ–º DespawnRemote –¥–ª—è –≤—ã—Ö–æ–¥–∞...")
        local despawnSuccess, despawnError = pcall(function()
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DespawnRemote"):InvokeServer()
            return true
        end)
        
        if despawnSuccess then
            print("‚úÖ DespawnRemote —É—Å–ø–µ—à–Ω–æ –≤—ã–∑–≤–∞–Ω")
        else
            print("‚ùå –û—à–∏–±–∫–∞ DespawnRemote: " .. tostring(despawnError))
        end
        
        wait(3) -- –ñ–¥–µ–º –ø–æ—Å–ª–µ –¥–µ—Å–ø–∞–≤–Ω–∞
        
        -- 2. –£–î–ê–õ–ï–ù–ò–ï –°–õ–û–¢–ê
        print("üóëÔ∏è –£–¥–∞–ª—è–µ–º —Å–ª–æ—Ç Slot1...")
        local deleteSuccess, deleteError = pcall(function()
            local args = {"Slot1", false}
            ReplicatedStorage.Remotes.DeleteSlotRemote:InvokeServer(unpack(args))
            return true
        end)
        
        if deleteSuccess then
            print("‚úÖ –ó–∞–ø—Ä–æ—Å —É–¥–∞–ª–µ–Ω–∏—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        else
            print("‚ùå –û—à–∏–±–∫–∞ —É–¥–∞–ª–µ–Ω–∏—è: " .. tostring(deleteError))
        end
        
        wait(5) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        
        -- 3. –°–û–ó–î–ê–ù–ò–ï –ù–û–í–û–ì–û –°–õ–û–¢–ê
        print("‚ûï –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—ã–π —Å–ª–æ—Ç Sochuri...")
        local createSuccess, createError = pcall(function()
            local args = {"Sochuri"}
            ReplicatedStorage.Remotes.CreateSlotRemote:InvokeServer(unpack(args))
            return true
        end)
        
        if createSuccess then
            print("‚úÖ –ó–∞–ø—Ä–æ—Å —Å–æ–∑–¥–∞–Ω–∏—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        else
            print("‚ùå –û—à–∏–±–∫–∞ —Å–æ–∑–¥–∞–Ω–∏—è: " .. tostring(createError))
        end
        
        wait(5) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        
        -- 4. –í–´–ü–û–õ–ù–Ø–ï–ú –ö–õ–ò–ö–ò
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º –∫–ª–∏–∫–∏ –ø–æ—Å–ª–µ —Å–æ–∑–¥–∞–Ω–∏—è —Å–ª–æ—Ç–∞...")
        local clickSuccess = clickAfterSlotCreation()
        
        if clickSuccess then
            print("üéØ –ö–ª–∏–∫–∏ –≤—ã–ø–æ–ª–Ω–µ–Ω—ã!")
            print("‚úÖ –û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–º–µ—Ä—Ç–∏ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!")
            return true
        else
            print("‚ö†Ô∏è –ë—ã–ª–∏ –ø—Ä–æ–±–ª–µ–º—ã —Å –∫–ª–∏–∫–∞–º–∏")
            return true
        end
    end
    
    return false
end

-- ========== –ù–ê–®–ò –ö–õ–ò–ö–ò (–î–õ–Ø –ù–û–†–ú–ê–õ–¨–ù–û–ì–û –ó–ê–ü–£–°–ö–ê) ==========
local function doOurClicks()
    print("\nüéØ –í—ã–ø–æ–ª–Ω—è–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏...")
    
    local player = game.Players.LocalPlayer
    local playerGui = player.PlayerGui
    
    print("‚è≥ –ñ–¥–µ–º 7 —Å–µ–∫—É–Ω–¥ –ø–µ—Ä–µ–¥ –∫–ª–∏–∫–∞–º–∏...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    wait(7)
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–ø–µ—Ü–∏–∞–ª—å–Ω—ã–µ GUI
    handleSpecialGUIs()
    
    -- –°–Ω–∞—á–∞–ª–∞ –ø–æ–ø—Ä–æ–±—É–µ–º –æ—Ç–∫—Ä—ã—Ç—å SaveSelectionGui
    local saveSelectionGui = playerGui:FindFirstChild("SaveSelectionGui")
    
    if not saveSelectionGui or not saveSelectionGui.Enabled then
        print("üìÇ SaveSelectionGui –Ω–µ –æ—Ç–∫—Ä—ã—Ç, –æ—Ç–∫—Ä—ã–≤–∞–µ–º...")
        click() -- –ö–ª–∏–∫ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞
        wait(3) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    end
    
    -- –¢–µ–ø–µ—Ä—å –∏—â–µ–º —ç–ª–µ–º–µ–Ω—Ç—ã
    saveSelectionGui = playerGui:FindFirstChild("SaveSelectionGui")
    
    -- 1. –ö–ª–∏–∫ –ø–æ –ø–µ—Ä–≤–æ–º—É —ç–ª–µ–º–µ–Ω—Ç—É
    print("1. –ò—â–µ–º –ø–µ—Ä–≤—ã–π —ç–ª–µ–º–µ–Ω—Ç (PagesFrame['1'])...")
    
    local elem1 = nil
    if saveSelectionGui and saveSelectionGui.Enabled then
        elem1 = saveSelectionGui.ContainerFrame.AllSlotsFrame.PagesFrame["1"]
    end
    
    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–ª—É—á—à–µ–Ω–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –∫–ª–∏–∫–∞
    local clicked1 = clickButtonEnhanced(elem1, "PagesFrame['1']")
    
    if not clicked1 then
        print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ PagesFrame['1'], –∫–ª–∏–∫–∞–µ–º –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
        click()
    end
    
    wait(3) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    
    -- 2. –ö–ª–∏–∫ –ø–æ PlayButton.UpperLabel
    print("\n2. –ò—â–µ–º –∫–Ω–æ–ø–∫—É PlayButton.UpperLabel...")
    
    local playButtonUpperLabel = nil
    if saveSelectionGui and saveSelectionGui.Enabled then
        -- –ò—â–µ–º —á–µ—Ä–µ–∑ –±–µ–∑–æ–ø–∞—Å–Ω—ã–π –ø–æ–∏—Å–∫
        local success, result = pcall(function()
            return saveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame["1"].InnerFrame.CreatureFrame.ButtonsFrame.PlayButton.UpperLabel
        end)
        
        if success and result then
            playButtonUpperLabel = result
        else
            -- –ü—Ä–æ–±—É–µ–º –∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –ø—É—Ç—å
            local success2, result2 = pcall(function()
                return saveSelectionGui:FindFirstChild("ContainerFrame", true):FindFirstChild("AllSlotsFrame", true):FindFirstChild("SlotsFrame", true):FindFirstChild("1", true):FindFirstChild("InnerFrame", true):FindFirstChild("CreatureFrame", true):FindFirstChild("ButtonsFrame", true):FindFirstChild("PlayButton", true):FindFirstChild("UpperLabel", true)
            end)
            
            if success2 and result2 then
                playButtonUpperLabel = result2
            end
        end
    end
    
    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —É–ª—É—á—à–µ–Ω–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –∫–ª–∏–∫–∞
    local clickedPlayButton = clickButtonEnhanced(playButtonUpperLabel, "PlayButton.UpperLabel")
    
    if clickedPlayButton then
        -- –í—Ç–æ—Ä–æ–π –∫–ª–∏–∫ —Å –Ω–µ–±–æ–ª—å—à–æ–π –∑–∞–¥–µ—Ä–∂–∫–æ–π
        wait(0.5)
        click()
    else
        print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ PlayButton.UpperLabel, –∏—â–µ–º —Å–∞–º—É –∫–Ω–æ–ø–∫—É PlayButton...")
        
        -- –ü—Ä–æ–±—É–µ–º –Ω–∞–π—Ç–∏ —Å–∞–º—É –∫–Ω–æ–ø–∫—É PlayButton
        local playButton = nil
        if saveSelectionGui and saveSelectionGui.Enabled then
            local success, result = pcall(function()
                return saveSelectionGui.ContainerFrame.AllSlotsFrame.SlotsFrame["1"].InnerFrame.CreatureFrame.ButtonsFrame.PlayButton
            end)
            
            if success and result then
                playButton = result
                local clickedPlay = clickButtonEnhanced(playButton, "PlayButton")
                
                if clickedPlay then
                    wait(0.5)
                    click()
                else
                    print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –∫–ª–∏–∫–Ω—É—Ç—å –ø–æ PlayButton, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
                    click()
                    wait(0.5)
                    click()
                end
            else
                print("‚ö†Ô∏è PlayButton –Ω–µ –Ω–∞–π–¥–µ–Ω, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
                click()
                wait(0.5)
                click()
            end
        else
            print("‚ö†Ô∏è SaveSelectionGui –Ω–µ –∞–∫—Ç–∏–≤–µ–Ω, –¥–µ–ª–∞–µ–º 2 –∫–ª–∏–∫–∞ –≤ —Ü–µ–Ω—Ç—Ä —ç–∫—Ä–∞–Ω–∞")
            click()
            wait(0.5)
            click()
        end
    end
    
    -- –ó–∞–∫—Ä—ã–≤–∞–µ–º SaveSelectionGui –µ—Å–ª–∏ –æ—Ç–∫—Ä—ã–ª–∏ –µ–≥–æ
    if saveSelectionGui and saveSelectionGui.Enabled then
        print("üìÇ –ó–∞–∫—Ä—ã–≤–∞–µ–º SaveSelectionGui...")
        click()
    end
    
    return true
end

-- ========== –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ü–ï–†–ò–û–î–ò–ß–ï–°–ö–ò–• REMOTE –ó–ê–ü–†–û–°–û–í ==========
local function periodicRemoteRequests()
    print("‚è∞ –ó–∞–ø—É—Å–∫–∞–µ–º –ø–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∏–µ Remote –∑–∞–ø—Ä–æ—Å—ã...")
    
    while true do
        wait(10) -- –ö–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
        
        print("\nüîÑ –í—ã–ø–æ–ª–Ω—è–µ–º –ø–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∏–µ Remote –∑–∞–ø—Ä–æ—Å—ã...")
        
        -- ClaimDailyLoginRemote
        local success1, error1 = pcall(function()
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ClaimDailyLoginRemote"):FireServer()
            print("‚úÖ ClaimDailyLoginRemote –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        end)
        
        if not success1 then
            print("‚ö†Ô∏è –û—à–∏–±–∫–∞ ClaimDailyLoginRemote: " .. tostring(error1))
        end
        
        wait(0.7)
        
        -- ClaimAdventCalendarRemote
        local success2, error2 = pcall(function()
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ClaimAdventCalendarRemote"):FireServer()
            print("‚úÖ ClaimAdventCalendarRemote –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        end)
        
        if not success2 then
            print("‚ö†Ô∏è –û—à–∏–±–∫–∞ ClaimAdventCalendarRemote: " .. tostring(error2))
        end
        
        -- GetSpawnedTokenRemote
        local success3, error3 = pcall(function()
            ReplicatedStorage.Remotes.GetSpawnedTokenRemote:InvokeServer()
            print("‚úÖ GetSpawnedTokenRemote –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
        end)
        
        if not success3 then
            print("‚ö†Ô∏è –û—à–∏–±–∫–∞ GetSpawnedTokenRemote: " .. tostring(error3))
        end
        
        print("‚úÖ –ü–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∏–µ –∑–∞–ø—Ä–æ—Å—ã –≤—ã–ø–æ–ª–Ω–µ–Ω—ã")
    end
end

-- ========== –§–ê–†–ú –ú–ò–°–°–ò–ô (–£–õ–£–ß–®–ï–ù–ù–ê–Ø –í–ï–†–°–ò–Ø) ==========
local function startMissionFarm()
    print("\nüöÄ –ó–∞–ø—É—Å–∫–∞–µ–º —É–ª—É—á—à–µ–Ω–Ω—ã–π —Ñ–∞—Ä–º –º–∏—Å—Å–∏–π...")
    
    local targetPlayer = game.Players.LocalPlayer
    
    print("‚è≥ –û–∂–∏–¥–∞–µ–º –∑–∞–≥—Ä—É–∑–∫–∏ –¥–∞–Ω–Ω—ã—Ö...")
    local playerGui = targetPlayer:WaitForChild("PlayerGui")
    local data = playerGui:WaitForChild("Data")
    local missions = data:WaitForChild("Missions")
    local regionMissions = missions:WaitForChild("RegionMissions")
    
    -- –ñ–¥–µ–º —Ä–µ–º–æ—É—Ç—ã
    local ChangeRegionRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("ChangeRegionRemote")
    local SetMissionRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("SetMissionRemote")
    local MudRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Mud")
    local FoodRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Food")
    local DrinkRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("DrinkRemote")
    local NPCDamageRemoteBreath = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("NPCDamageRemoteBreath")
    
    -- –ö–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
    local teleportPositions = {
        Vector3.new(2418.49072, 332.279358, -118.064125),
        Vector3.new(2518.49072, 332.279358, -118.064125),
        Vector3.new(2318.49072, 332.279358, -118.064125)
    }
    
    local concealScentPosition = Vector3.new(2434.90479, 277.143066, -1289.48108)
    
    -- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ
    local lastFoodDrinkTime = 0
    local lastAttackTime = 0
    local lastDeathCheck = 0
    
    -- –°–ø–∏—Å–æ–∫ –º–∏—Å—Å–∏–π –∫–æ—Ç–æ—Ä—ã–µ –Ω—É–∂–Ω–æ –∏–≥–Ω–æ—Ä–∏—Ä–æ–≤–∞—Ç—å
    local ignoreMissions = {
        ShoomPilesCollected = true
    }
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –Ω–µ–≤—ã–ø–æ–ª–Ω–µ–Ω–Ω—ã—Ö –º–∏—Å—Å–∏–π
    function getUncompletedMissions()
        local uncompleted = {}
        
        for _, region in pairs(regionMissions:GetChildren()) do
            for _, mission in pairs(region:GetChildren()) do
                if mission:IsA("BoolValue") and not mission.Value then
                    -- –ü—Ä–æ–ø—É—Å–∫–∞–µ–º –º–∏—Å—Å–∏–∏ –∏–∑ —Å–ø–∏—Å–∫–∞ –∏–≥–Ω–æ—Ä–∏—Ä—É–µ–º—ã—Ö
                    if not ignoreMissions[mission.Name] then
                        table.insert(uncompleted, {
                            regionName = region.Name,
                            missionName = mission.Name,
                            missionObject = mission
                        })
                    else
                        print("‚è≠Ô∏è –ü—Ä–æ–ø—É—Å–∫–∞–µ–º –º–∏—Å—Å–∏—é: " .. region.Name .. "." .. mission.Name)
                    end
                end
            end
        end
        
        return uncompleted
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–º–µ–Ω—ã —Ä–µ–≥–∏–æ–Ω–∞
    function changeRegion(regionName)
        print("üìç –ú–µ–Ω—è–µ–º —Ä–µ–≥–∏–æ–Ω –Ω–∞: " .. regionName)
        local args = { regionName }
        ChangeRegionRemote:FireServer(unpack(args))
        wait(1)
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –∏–≥—Ä–æ–∫–∞
    function teleportPlayer(position)
        local character = targetPlayer.Character
        if character then
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                humanoidRootPart.CFrame = CFrame.new(position)
                print("üìç –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–ª–∏—Å—å –≤: " .. tostring(position))
            end
        end
    end
    
    -- –£–õ–£–ß–®–ï–ù–ù–ê–Ø –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ò–°–ü–û–õ–¨–ó–û–í–ê–ù–ò–Ø –ï–î–´ –ò –í–û–î–´
    function useFoodAndWater()
        local currentTime = tick()
        if currentTime - lastFoodDrinkTime >= 2 then
            -- –ò–°–ü–†–ê–í–õ–ï–ù–ù–´–ô –ü–û–ò–°–ö –ï–î–´: –ò—Å–ø–æ–ª—å–∑—É–µ–º —Ä–∞–Ω–¥–æ–º–Ω—ã–π —ç–ª–µ–º–µ–Ω—Ç –∏–∑ —Å–ø–∏—Å–∫–∞ –µ–¥—ã
            local foodFolder = workspace.Interactions.Food
            local foodChildren = foodFolder:GetChildren()
            
            if #foodChildren > 0 then
                -- –ë–µ—Ä–µ–º —Å–ª—É—á–∞–π–Ω—ã–π —ç–ª–µ–º–µ–Ω—Ç
                local randomIndex = math.random(1, #foodChildren)
                local foodItem = foodChildren[randomIndex]
                
                if foodItem then
                    local foodArgs = { foodItem }
                    FoodRemote:FireServer(unpack(foodArgs))
                    print("üçé –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∞ –µ–¥–∞: " .. foodItem.Name .. " (—Å–ª—É—á–∞–π–Ω—ã–π —ç–ª–µ–º–µ–Ω—Ç #" .. randomIndex .. ")")
                else
                    print("‚ö†Ô∏è –†–∞–Ω–¥–æ–º–Ω—ã–π —ç–ª–µ–º–µ–Ω—Ç –µ–¥—ã –Ω–µ –Ω–∞–π–¥–µ–Ω")
                end
            else
                print("‚ùå –í –ø–∞–ø–∫–µ Food –Ω–µ—Ç —ç–ª–µ–º–µ–Ω—Ç–æ–≤")
            end
            
            -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –≤–æ–¥—É
            local lake = workspace.Interactions.Lakes:FindFirstChild("Lake")
            if lake then
                local drinkArgs = { lake }
                DrinkRemote:FireServer(unpack(drinkArgs))
                print("üíß –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∞ –≤–æ–¥–∞")
            else
                -- –ü—Ä–æ–±—É–µ–º –Ω–∞–π—Ç–∏ –∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –∏—Å—Ç–æ—á–Ω–∏–∫ –≤–æ–¥—ã
                local waterSource = workspace.Interactions.Lakes:FindFirstChildOfClass("Part")
                if waterSource then
                    local drinkArgs = { waterSource }
                    DrinkRemote:FireServer(unpack(drinkArgs))
                    print("üíß –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω –∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –∏—Å—Ç–æ—á–Ω–∏–∫ –≤–æ–¥—ã: " .. waterSource.Name)
                else
                    print("‚ö†Ô∏è –í–æ–¥–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω–∞")
                end
            end
            
            lastFoodDrinkTime = currentTime
        end
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –±–ª–∏–∂–∞–π—à–µ–≥–æ Mud –æ–±—ä–µ–∫—Ç–∞
    function findNearestMud()
        local character = targetPlayer.Character
        if not character then return nil end
        
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if not humanoidRootPart then return nil end
        
        local playerPos = humanoidRootPart.Position
        local nearestMud = nil
        local nearestDistance = math.huge
        
        local mudFolder = workspace.Interactions.Mud
        if mudFolder then
            for _, mudObject in pairs(mudFolder:GetChildren()) do
                if mudObject:IsA("Part") or mudObject:IsA("MeshPart") then
                    local distance = (playerPos - mudObject.Position).Magnitude
                    if distance < nearestDistance then
                        nearestDistance = distance
                        nearestMud = mudObject
                    end
                end
            end
        end
        
        return nearestMud
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ NPC –¥–ª—è –∞—Ç–∞–∫–∏
    function findNPCForAttack()
        local rootsNPC = workspace.Interactions.NPCs:FindFirstChild("Roots")
        if rootsNPC then
            local rootPart = rootsNPC:FindFirstChild("RootPart")
            if rootPart then
                return rootPart
            end
        end
        
        for _, npc in pairs(workspace.Interactions.NPCs:GetChildren()) do
            local rootPart = npc:FindFirstChild("RootPart") or npc:FindFirstChild("HumanoidRootPart")
            if rootPart then
                return rootPart
            end
        end
        
        return nil
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –∫ NPC —Å–ø–∞–≤–Ω—É (–Ω–∞ 30 –µ–¥–∏–Ω–∏—Ü –≤—ã—à–µ)
    function teleportToRandomNPCSpawn()
        local npcSpawns = workspace.Interactions.NPCSpawns:GetChildren()
        if #npcSpawns > 0 then
            local randomSpawn = npcSpawns[math.random(1, #npcSpawns)]
            if randomSpawn then
                local spawnPosition = randomSpawn.Position + Vector3.new(0, 30, 0)
                teleportPlayer(spawnPosition)
                return true
            end
        end
        return false
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞—Ç–∞–∫–∏ NPC
    function attackNPC()
        local currentTime = tick()
        if currentTime - lastAttackTime >= 0.5 then
            local targetNPC = findNPCForAttack()
            if targetNPC then
                local attackArgs = { { targetNPC } }
                NPCDamageRemoteBreath:FireServer(unpack(attackArgs))
                lastAttackTime = currentTime
                print("‚öîÔ∏è –ê—Ç–∞–∫–æ–≤–∞–Ω NPC: " .. targetNPC.Parent.Name)
                return true
            else
                print("‚ö†Ô∏è NPC –¥–ª—è –∞—Ç–∞–∫–∏ –Ω–µ –Ω–∞–π–¥–µ–Ω")
            end
        end
        return false
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ Sniff
    function completeSniffMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º Sniff –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 40 do
            attempts = attempts + 1
            
            useFoodAndWater()
            
            local missionArgs = { 1 }
            SetMissionRemote:FireServer(unpack(missionArgs))
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local sniffMission = regionFolder:FindFirstChild("Sniff")
                if sniffMission and sniffMission.Value then
                    missionCompleted = true
                    print("‚úÖ Sniff –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                else
                    print("‚è≥ Sniff... (" .. attempts .. "/40)")
                end
            end
            
            if not missionCompleted then
                wait(1.5)
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ ConcealScent
    function completeConcealScentMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º ConcealScent –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        teleportPlayer(concealScentPosition)
        wait(2)
        
        changeRegion(regionName) -- –ü–æ–≤—Ç–æ—Ä–Ω–æ —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ä–µ–≥–∏–æ–Ω –ø–æ—Å–ª–µ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 60 do
            attempts = attempts + 1
            
            useFoodAndWater()
            
            local nearestMud = findNearestMud()
            if nearestMud then
                local mudArgs = { nearestMud }
                MudRemote:FireServer(unpack(mudArgs))
                print("üõÅ –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω Mud: " .. nearestMud.Name)
            else
                print("‚ö†Ô∏è Mud –Ω–µ –Ω–∞–π–¥–µ–Ω")
            end
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local concealMission = regionFolder:FindFirstChild("ConcealScent")
                if concealMission and concealMission.Value then
                    missionCompleted = true
                    print("‚úÖ ConcealScent –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                else
                    print("‚è≥ ConcealScent... (" .. attempts .. "/60)")
                end
            end
            
            if not missionCompleted then
                wait(1)
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ EatFoodDrinkWater
    function completeEatFoodDrinkWaterMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º EatFoodDrinkWater –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 30 do
            attempts = attempts + 1
            
            useFoodAndWater()
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local eatMission = regionFolder:FindFirstChild("EatFoodDrinkWater")
                if eatMission and eatMission.Value then
                    missionCompleted = true
                    print("‚úÖ EatFoodDrinkWater –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                else
                    print("‚è≥ EatFoodDrinkWater... (" .. attempts .. "/30)")
                end
            end
            
            if not missionCompleted then
                wait(2)
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ AttackOrHealCreatureOrNPC
    function completeAttackOrHealMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º AttackOrHealCreatureOrNPC –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        if teleportToRandomNPCSpawn() then
            wait(2)
            changeRegion(regionName) -- –ü–æ–≤—Ç–æ—Ä–Ω–æ —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ä–µ–≥–∏–æ–Ω –ø–æ—Å–ª–µ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
        end
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 100 do
            attempts = attempts + 1
            
            useFoodAndWater()
            attackNPC()
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ –ö–ê–ñ–î–´–ô —Ä–∞–∑ –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–µ–π –∞—Ç–∞–∫–æ–π
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local attackMission = regionFolder:FindFirstChild("AttackOrHealCreatureOrNPC")
                if attackMission and attackMission.Value then
                    missionCompleted = true
                    print("‚úÖ AttackOrHealCreatureOrNPC –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                    break -- –ù–µ–º–µ–¥–ª–µ–Ω–Ω–æ –≤—ã—Ö–æ–¥–∏–º –∏–∑ —Ü–∏–∫–ª–∞
                end
            end
            
            if not missionCompleted then
                wait(0.5) -- –ö–î 0.5 —Å–µ–∫—É–Ω–¥—ã –º–µ–∂–¥—É –∞—Ç–∞–∫–∞–º–∏
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ DistanceTravelled
    function completeDistanceTravelledMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º DistanceTravelled –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local attempts = 0
        
        while not missionCompleted and attempts < 100 do
            attempts = attempts + 1
            
            useFoodAndWater()
            
            for i, pos in ipairs(teleportPositions) do
                teleportPlayer(pos)
                wait(2)
                
                if i == 1 then
                    changeRegion(regionName) -- –ü–æ–≤—Ç–æ—Ä–Ω–æ —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ä–µ–≥–∏–æ–Ω –ø–æ—Å–ª–µ –ø–µ—Ä–≤–æ–π —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
                end
                
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ –ø–æ—Å–ª–µ –∫–∞–∂–¥–æ–π —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
                local regionFolder = regionMissions:FindFirstChild(regionName)
                if regionFolder then
                    local distanceMission = regionFolder:FindFirstChild("DistanceTravelled")
                    if distanceMission and distanceMission.Value then
                        missionCompleted = true
                        print("‚úÖ DistanceTravelled –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                        break
                    end
                end
            end
            
            if missionCompleted then
                break
            else
                print("‚è≥ DistanceTravelled... (" .. attempts .. "/100)")
            end
        end
        
        return missionCompleted
    end
    
    -- –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –º–∏—Å—Å–∏–∏ TimePlayed
    function completeTimePlayedMission(regionName)
        print("üîÑ –í—ã–ø–æ–ª–Ω—è–µ–º TimePlayed –≤ —Ä–µ–≥–∏–æ–Ω–µ: " .. regionName)
        print("‚è∞ –û–∂–∏–¥–∞–µ–º...")
        
        changeRegion(regionName)
        
        local missionCompleted = false
        local checkCount = 0
        
        while not missionCompleted do
            checkCount = checkCount + 1
            useFoodAndWater()
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ –ø–æ—è–≤–∏–ª–∏—Å—å –ª–∏ –¥—Ä—É–≥–∏–µ –º–∏—Å—Å–∏–∏
            local otherMissions = getUncompletedMissions()
            for _, mission in ipairs(otherMissions) do
                if mission.missionName ~= "TimePlayed" then
                    print("‚ö†Ô∏è –ü–æ—è–≤–∏–ª–∏—Å—å –¥—Ä—É–≥–∏–µ –º–∏—Å—Å–∏–∏! –ü—Ä–µ—Ä—ã–≤–∞–µ–º TimePlayed")
                    return false
                end
            end
            
            local regionFolder = regionMissions:FindFirstChild(regionName)
            if regionFolder then
                local timeMission = regionFolder:FindFirstChild("TimePlayed")
                if timeMission and timeMission.Value then
                    missionCompleted = true
                    print("‚úÖ TimePlayed –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ –≤ —Ä–µ–≥–∏–æ–Ω–µ " .. regionName)
                    break
                else
                    print("‚è≥ TimePlayed... (" .. checkCount .. " –ø—Ä–æ–≤–µ—Ä–æ–∫)")
                end
            end
            
            wait(10)
        end
        
        return missionCompleted
    end
    
    -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Ñ–∞—Ä–º–∞
    function autoCompleteAllMissions()
        print("üöÄ –ó–∞–ø—É—Å–∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è –Ω–µ–≤—ã–ø–æ–ª–Ω–µ–Ω–Ω—ã—Ö –º–∏—Å—Å–∏–π...")
        
        while true do
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å –≤–æ –≤—Ä–µ–º—è —Ñ–∞—Ä–º–∞
            if checkDeathDuringFarm() then
                print("üîÑ –ü–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏ –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º —Ñ–∞—Ä–º...")
                wait(5)
            end
            
            local uncompleted = getUncompletedMissions()
            
            if #uncompleted == 0 then
                print("üéâ –í—Å–µ –º–∏—Å—Å–∏–∏ –≤—ã–ø–æ–ª–Ω–µ–Ω—ã!")
                wait(10)
            else
                print("üìã –ù–∞–π–¥–µ–Ω–æ –Ω–µ–≤—ã–ø–æ–ª–Ω–µ–Ω–Ω—ã—Ö –º–∏—Å—Å–∏–π: " .. #uncompleted)
                for i, mission in ipairs(uncompleted) do
                    print(i .. ". " .. mission.regionName .. " - " .. mission.missionName)
                end
                
                for i, mission in ipairs(uncompleted) do
                    print("\nüéØ –ú–∏—Å—Å–∏—è " .. i .. "/" .. #uncompleted .. ": " .. mission.regionName .. " - " .. mission.missionName)
                    
                    local success = false
                    
                    if mission.missionName == "Sniff" then
                        success = completeSniffMission(mission.regionName)
                    elseif mission.missionName == "ConcealScent" then
                        success = completeConcealScentMission(mission.regionName)
                    elseif mission.missionName == "EatFoodDrinkWater" then
                        success = completeEatFoodDrinkWaterMission(mission.regionName)
                    elseif mission.missionName == "AttackOrHealCreatureOrNPC" then
                        success = completeAttackOrHealMission(mission.regionName)
                    elseif mission.missionName == "DistanceTravelled" then
                        success = completeDistanceTravelledMission(mission.regionName)
                    elseif mission.missionName == "TimePlayed" then
                        success = completeTimePlayedMission(mission.regionName)
                    else
                        print("‚ùì –ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π —Ç–∏–ø –º–∏—Å—Å–∏–∏: " .. mission.missionName)
                    end
                    
                    if success then
                        print("‚úÖ –£—Å–ø–µ—à–Ω–æ –≤—ã–ø–æ–ª–Ω–µ–Ω–æ!")
                        
                        -- –ü–æ—Å–ª–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è –º–∏—Å—Å–∏–∏ –ø—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ –ø–æ—è–≤–∏–ª–∏—Å—å –ª–∏ –¥—Ä—É–≥–∏–µ
                        local newMissions = getUncompletedMissions()
                        if #newMissions > 0 then
                            print("üîÑ –û–±–Ω–∞—Ä—É–∂–µ–Ω—ã –Ω–æ–≤—ã–µ –º–∏—Å—Å–∏–∏, –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º...")
                        end
                    else
                        print("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –≤—ã–ø–æ–ª–Ω–∏—Ç—å")
                    end
                    
                    wait(1)
                end
                
                print("\nüîÑ –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ –Ω–æ–≤—ã—Ö –º–∏—Å—Å–∏–π...")
                wait(5)
            end
        end
    end
    
    -- –ó–∞–ø—É—Å–∫–∞–µ–º –ø–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∏–µ –∑–∞–ø—Ä–æ—Å—ã –≤ –æ—Ç–¥–µ–ª—å–Ω–æ–º –ø–æ—Ç–æ–∫–µ
    spawn(periodicRemoteRequests)
    
    -- –ó–∞–ø—É—Å–∫–∞–µ–º —Ñ–∞—Ä–º –º–∏—Å—Å–∏–π
    autoCompleteAllMissions()
end

-- ========== –ì–õ–ê–í–ù–ê–Ø –õ–û–ì–ò–ö–ê ==========
print("\n" .. string.rep("=", 50))
print("üîß –ó–ê–ü–£–°–ö –°–ö–†–ò–ü–¢–ê")
print(string.rep("=", 50))

print("\nüîß –®–∞–≥ 0: –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–º–µ—Ä—Ç—å...")
local deathHandled = handleDeath()

if deathHandled then
    print("‚è≥ –û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–º–µ—Ä—Ç–∏ –∑–∞–≤–µ—Ä—à–µ–Ω–∞, –ø—Ä–æ–≤–µ—Ä—è–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª...")
    
    -- –ü–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏ –ø—Ä–æ–≤–µ—Ä—è–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª
    print("\nüîß –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª...")
    local tutorialHandled = handleTutorial()
    
    if tutorialHandled then
        print("‚è≥ –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        wait(3)
    end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–ø–µ—Ü–∏–∞–ª—å–Ω—ã–µ GUI
    handleSpecialGUIs()
    
    print("\n" .. string.rep("=", 50))
    print("üîß –ó–∞–ø—É—Å–∫–∞–µ–º —Ñ–∞—Ä–º –º–∏—Å—Å–∏–π")
    print(string.rep("=", 50))
    print("‚è≥ –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    wait(3)
    
    startMissionFarm()
    
else
    -- –ï—Å–ª–∏ —Å–º–µ—Ä—Ç–∏ –Ω–µ –±—ã–ª–æ, –≤—ã–ø–æ–ª–Ω—è–µ–º –æ–±—ã—á–Ω—ã–π –ø–æ—Ä—è–¥–æ–∫
    print("\nüîß –®–∞–≥ 1: –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ç—É—Ç–æ—Ä–∏–∞–ª...")
    local tutorialHandled = handleTutorial()
    
    if tutorialHandled then
        print("‚è≥ –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
        wait(3)
    end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–ø–µ—Ü–∏–∞–ª—å–Ω—ã–µ GUI
    handleSpecialGUIs()
    
    print("\nüîß –®–∞–≥ 2: –í—ã–ø–æ–ª–Ω—è–µ–º –Ω–∞—à–∏ –∫–ª–∏–∫–∏...")
    doOurClicks()
    
    print("\n" .. string.rep("=", 50))
    print("üîß –®–∞–≥ 3: –ó–∞–ø—É—Å–∫–∞–µ–º —Ñ–∞—Ä–º –º–∏—Å—Å–∏–π")
    print(string.rep("=", 50))
    print("‚è≥ –ñ–¥–µ–º 3 —Å–µ–∫—É–Ω–¥—ã...") -- –£–≤–µ–ª–∏—á–µ–Ω–Ω—ã–π –ö–î
    wait(3)
    
    startMissionFarm()
end

print("\n‚ú® –°–ö–†–ò–ü–¢ –ó–ê–í–ï–†–®–ï–ù!")
