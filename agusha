-- –°–∫—Ä–∏–ø—Ç –¥–ª—è —Ç–æ–≥–æ –∫—Ç–æ –ü–û–õ–£–ß–ê–ï–¢ —Ç–æ–∫–µ–Ω—ã
-- –≠—Ç–æ—Ç —Å–∫—Ä–∏–ø—Ç —Ç–æ–ª—å–∫–æ –ø—Ä–∏–Ω–∏–º–∞–µ—Ç —Ç—Ä–µ–π–¥—ã –æ—Ç –≤—Å–µ—Ö

local placeId = 14119723130

if game.PlaceId ~= placeId then
    game:GetService("TeleportService"):Teleport(placeId)
    return
end

print("üîÑ –ê–≤—Ç–æ–ø—Ä–∏–Ω—è—Ç–∏–µ —Ç—Ä–µ–π–¥–æ–≤ –∑–∞–ø—É—â–µ–Ω–æ")

-- –ü—Ä–∏–Ω–∏–º–∞–µ–º –∑–∞–ø—Ä–æ—Å—ã –æ—Ç –í–°–ï–• –∏–≥—Ä–æ–∫–æ–≤
local function acceptAllTrades()
    while wait(0.3) do
        -- –ü—Ä–∏–Ω–∏–º–∞–µ–º –≤—Å–µ –≤—Ö–æ–¥—è—â–∏–µ –∑–∞–ø—Ä–æ—Å—ã
        local args = {"AcceptRequest"}
        game:GetService("Players").LocalPlayer:WaitForChild("Remotes"):WaitForChild("TradeRequestRemote"):FireServer(unpack(args))
    end
end

-- –ê–≤—Ç–æ–ø—Ä–∏–Ω—è—Ç–∏–µ Galaxy —Ç–æ–∫–µ–Ω–æ–≤
local function autoAcceptGalaxy()
    while wait(0.3) do
        local tradeGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("TradeGui")
        
        if tradeGui and tradeGui.Enabled then
            local senderName = tradeGui.ContainerFrame.Theirs.DisplayNameLabel.Text
            
            if senderName and senderName ~= "" then
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –µ—Å—Ç—å –ª–∏ Galaxy —Ç–æ–∫–µ–Ω—ã
                local hasGalaxyToken = false
                local offersFrame = tradeGui.ContainerFrame.Theirs.OffersFrame.ScrollingFrame
                
                if offersFrame then
                    for _, child in pairs(offersFrame:GetChildren()) do
                        if child:IsA("Frame") and child.Name == "Default" then
                            local nameLabel = child:FindFirstChild("NameLabel")
                            if nameLabel and nameLabel.Text:lower():find("galaxy") then
                                hasGalaxyToken = true
                                break
                            end
                        end
                    end
                end
                
                -- –ï—Å–ª–∏ –µ—Å—Ç—å Galaxy —Ç–æ–∫–µ–Ω - –ø—Ä–∏–Ω–∏–º–∞–µ–º —Ç—Ä–µ–π–¥
                if hasGalaxyToken then
                    local remoteName = senderName .. "-" .. game.Players.LocalPlayer.Name .. "TradeRemote"
                    local remote = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):FindFirstChild(remoteName)
                    
                    if remote then
                        -- –°–ø–∞–º–∏–º AcceptTrade
                        for i = 1, 10 do
                            local args = {"AcceptTrade"}
                            remote:InvokeServer(unpack(args))
                        end
                        print("‚úÖ –ü—Ä–∏–Ω—è—Ç —Ç—Ä–µ–π–¥ —Å Galaxy —Ç–æ–∫–µ–Ω–∞–º–∏ –æ—Ç: " .. senderName)
                    end
                end
            end
        end
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –æ–±–∞ –ø—Ä–æ—Ü–µ—Å—Å–∞
spawn(acceptAllTrades)
spawn(autoAcceptGalaxy)

print("‚úÖ –ì–æ—Ç–æ–≤ –ø—Ä–∏–Ω–∏–º–∞—Ç—å —Ç—Ä–µ–π–¥—ã –æ—Ç –í–°–ï–• –∏–≥—Ä–æ–∫–æ–≤!")
print("‚úÖ –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø—Ä–∏–Ω–∏–º–∞—é —Ç—Ä–µ–π–¥—ã —Å Galaxy —Ç–æ–∫–µ–Ω–∞–º–∏!")