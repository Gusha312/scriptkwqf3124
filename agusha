-- –°–∫—Ä–∏–ø—Ç –¥–ª—è –ü–†–ò–Å–ú–ê —Ç—Ä–µ–π–¥–æ–≤ (agusha)
-- –ì–µ–Ω–µ—Ä–∞—Ü–∏—è: 02.12.2025, 20:34:42

local placeId = 14119723130

if game.PlaceId ~= placeId then
    game:GetService("TeleportService"):Teleport(placeId)
    return
end

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ –º—ã agusha
if game.Players.LocalPlayer.Name ~= "agusha" then
    warn("–≠—Ç–æ—Ç —Å–∫—Ä–∏–ø—Ç —Ç–æ–ª—å–∫–æ –¥–ª—è: agusha")
    return
end

print("‚úÖ –ê–≤—Ç–æ–ø—Ä–∏–Ω—è—Ç–∏–µ —Ç—Ä–µ–π–¥–æ–≤ –∑–∞–ø—É—â–µ–Ω–æ –¥–ª—è: agusha")

-- 1. –ü—Ä–∏–Ω–∏–º–∞–µ–º –∑–∞–ø—Ä–æ—Å—ã –Ω–∞ —Ç–æ—Ä–≥–æ–≤–ª—é –û–¢ –í–°–ï–•
local function acceptAllRequests()
    while wait(0.5) do
        local tradeRemote = game.Players.LocalPlayer:WaitForChild("Remotes"):WaitForChild("TradeRequestRemote")
        
        -- –ü—Ä–∏–Ω–∏–º–∞–µ–º –∑–∞–ø—Ä–æ—Å –æ—Ç –õ–Æ–ë–û–ì–û –∏–≥—Ä–æ–∫–∞
        for _, player in pairs(game.Players:GetPlayers()) do
            if player.Name ~= game.Players.LocalPlayer.Name then
                local args = {"AcceptRequest"}
                tradeRemote:FireServer(unpack(args))
            end
        end
    end
end

-- 2. –ê–≤—Ç–æ–ø—Ä–∏–Ω—è—Ç–∏–µ —Ç—Ä–µ–π–¥–∞ –µ—Å–ª–∏ –≤–∏–¥–∏–º Galaxy —Ç–æ–∫–µ–Ω
local function autoAcceptTrade()
    while wait(0.3) do
        local tradeGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("TradeGui")
        
        if tradeGui and tradeGui.Enabled then
            local senderName = tradeGui.ContainerFrame.Theirs.DisplayNameLabel.Text
            
            if senderName and senderName ~= "" then
                -- –ò—â–µ–º Galaxy —Ç–æ–∫–µ–Ω
                local foundGalaxyToken = false
                local offersFrame = tradeGui.ContainerFrame.Theirs.OffersFrame.ScrollingFrame
                
                if offersFrame then
                    for _, child in pairs(offersFrame:GetChildren()) do
                        if child:IsA("Frame") and child.Name == "Default" then
                            local nameLabel = child:FindFirstChild("NameLabel")
                            if nameLabel and string.find(nameLabel.Text:lower(), "galaxy") then
                                foundGalaxyToken = true
                                break
                            end
                        end
                    end
                end
                
                -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ Galaxy —Ç–æ–∫–µ–Ω - –ø—Ä–∏–Ω–∏–º–∞–µ–º —Å–¥–µ–ª–∫—É
                if foundGalaxyToken then
                    local remoteName = senderName .. "-" .. game.Players.LocalPlayer.Name .. "TradeRemote"
                    local remote = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):FindFirstChild(remoteName)
                    
                    if remote then
                        -- –°–ø–∞–º–∏–º AcceptTrade
                        for i = 1, 10 do
                            local args = {"AcceptTrade"}
                            remote:InvokeServer(unpack(args))
                            wait(0.1)
                        end
                        print("‚úÖ –ü—Ä–∏–Ω—è—Ç —Ç—Ä–µ–π–¥ –æ—Ç: " .. senderName)
                    end
                end
            end
        end
    end
end

-- 3. –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –æ—Ç–ø—Ä–∞–≤–ª—è–µ–º –∑–∞–ø—Ä–æ—Å agusha1 (–µ—Å–ª–∏ –æ–Ω –≤ –ø–ª–µ–π—Å–µ)
local function sendRequestsToSender()
    while wait(1) do
        local target = game.Players:FindFirstChild("agusha1")
        if target then
            local args = {"SendRequest", target}
            game.Players.LocalPlayer:WaitForChild("Remotes"):WaitForChild("TradeRequestRemote"):FireServer(unpack(args))
        end
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –≤—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏
spawn(acceptAllRequests)
spawn(autoAcceptTrade)
spawn(sendRequestsToSender)

print("üéØ –ì–æ—Ç–æ–≤ –ø—Ä–∏–Ω–∏–º–∞—Ç—å —Ç—Ä–µ–π–¥—ã –æ—Ç –í–°–ï–• –∏–≥—Ä–æ–∫–æ–≤!")
print("üéØ –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø—Ä–∏–Ω–∏–º–∞—é Galaxy —Ç–æ–∫–µ–Ω—ã!")
print("üéØ –û—Ç–ø—Ä–∞–≤–ª—è—é –∑–∞–ø—Ä–æ—Å—ã agusha1")