-- –°–∫—Ä–∏–ø—Ç –¥–ª—è agusha - –ü–†–ò–ù–ò–ú–ê–ï–¢ —Ç—Ä–µ–π–¥—ã –æ—Ç –≤—Å–µ—Ö
-- –≠—Ç–æ—Ç –∏–≥—Ä–æ–∫ –ø—Ä–∏–Ω–∏–º–∞–µ—Ç —Ç—Ä–µ–π–¥—ã, –Ω–æ –Ω–µ –æ—Ç–ø—Ä–∞–≤–ª—è–µ—Ç

local placeId = 14119723130

if game.PlaceId ~= placeId then
    game:GetService("TeleportService"):Teleport(placeId)
    return
end

print("üîÑ –ê–≤—Ç–æ–ø—Ä–∏–Ω—è—Ç–∏–µ —Ç—Ä–µ–π–¥–æ–≤ –∑–∞–ø—É—â–µ–Ω–æ")

-- 1. –ü—Ä–∏–Ω–∏–º–∞–µ–º –∑–∞–ø—Ä–æ—Å—ã –æ—Ç –í–°–ï–• –∏–≥—Ä–æ–∫–æ–≤ (–û–°–ù–û–í–ù–û–ô –ü–û–¢–û–ö)
spawn(function()
    while wait(0.5) do
        local tradeRemote = game.Players.LocalPlayer:WaitForChild("Remotes"):WaitForChild("TradeRequestRemote")
        local args = {"AcceptRequest"}
        tradeRemote:FireServer(unpack(args))
    end
end)

-- 2. –ê–≤—Ç–æ–ø—Ä–∏–Ω—è—Ç–∏–µ —Ç—Ä–µ–π–¥–æ–≤ —Å Galaxy —Ç–æ–∫–µ–Ω–∞–º–∏
spawn(function()
    while wait(0.3) do
        local tradeGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("TradeGui")
        
        if tradeGui and tradeGui.Enabled == true then
            -- –ü–æ–ª—É—á–∞–µ–º –∏–º—è –æ—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—è
            local senderName = tradeGui.ContainerFrame.Theirs.DisplayNameLabel.Text
            
            if senderName and senderName ~= "" then
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ –≤ —Ç—Ä–µ–π–¥–µ –µ—Å—Ç—å Galaxy —Ç–æ–∫–µ–Ω
                local foundGalaxyToken = false
                local offersFrame = tradeGui.ContainerFrame.Theirs.OffersFrame.ScrollingFrame
                
                if offersFrame then
                    for _, child in pairs(offersFrame:GetChildren()) do
                        if child:IsA("Frame") and child.Name == "Default" then
                            local nameLabel = child:FindFirstChild("NameLabel")
                            if nameLabel and (string.find(nameLabel.Text:lower(), "galaxy") or nameLabel.Text == "Galaxy Exploration Token") then
                                foundGalaxyToken = true
                                break
                            end
                        end
                    end
                end
                
                -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ Galaxy —Ç–æ–∫–µ–Ω - –ø—Ä–∏–Ω–∏–º–∞–µ–º —Ç—Ä–µ–π–¥
                if foundGalaxyToken then
                    local remoteName = senderName .. "-" .. game.Players.LocalPlayer.Name .. "TradeRemote"
                    local remote = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):FindFirstChild(remoteName)
                    
                    if remote then
                        -- –°–ø–∞–º–∏–º AcceptTrade 10 —Ä–∞–∑
                        for i = 1, 10 do
                            local args = {"AcceptTrade"}
                            remote:InvokeServer(unpack(args))
                            wait(0.1)
                        end
                        print("‚úÖ –ü—Ä–∏–Ω—è—Ç —Ç—Ä–µ–π–¥ —Å Galaxy —Ç–æ–∫–µ–Ω–æ–º –æ—Ç: " .. senderName)
                    end
                end
            end
        end
    end
end)

-- 3. –ü—Ä–æ—Å—Ç–æ –∂–¥–µ–º –∏ –ø—Ä–∏–Ω–∏–º–∞–µ–º –í–°–Å (—Ä–µ–∑–µ—Ä–≤–Ω—ã–π –º–µ—Ç–æ–¥)
spawn(function()
    while wait(1) do
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –≤—Å–µ –∞–∫—Ç–∏–≤–Ω—ã–µ —Ç—Ä–µ–π–¥—ã –∏ –ø—Ä–∏–Ω–∏–º–∞–µ–º –∏—Ö
        for _, player in pairs(game.Players:GetPlayers()) do
            if player.Name ~= game.Players.LocalPlayer.Name then
                -- –ü—ã—Ç–∞–µ–º—Å—è –ø—Ä–∏–Ω—è—Ç—å –ª—é–±–æ–π —Ç—Ä–µ–π–¥
                local remoteName = player.Name .. "-" .. game.Players.LocalPlayer.Name .. "TradeRemote"
                local remote = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):FindFirstChild(remoteName)
                
                if remote then
                    local args = {"AcceptTrade"}
                    remote:InvokeServer(unpack(args))
                end
            end
        end
    end
end)

print("‚úÖ –ì–æ—Ç–æ–≤ –ø—Ä–∏–Ω–∏–º–∞—Ç—å —Ç—Ä–µ–π–¥—ã –æ—Ç –õ–Æ–ë–û–ì–û –∏–≥—Ä–æ–∫–∞!")
print("‚úÖ –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø—Ä–∏–Ω–∏–º–∞—é —Ç—Ä–µ–π–¥—ã —Å Galaxy —Ç–æ–∫–µ–Ω–∞–º–∏!")